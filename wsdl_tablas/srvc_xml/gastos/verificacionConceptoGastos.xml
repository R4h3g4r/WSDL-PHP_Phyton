<?xml version = "1.0" encoding = "ISO-8859-1" ?>

<service xmlns = "http://wcorp/util/bee/serviciosdedatos"
         xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation = "http://wcorp/util/bee/serviciosdedatos
                              XMLService.xsd"
         name = "Mc2VerGasNot">

  <context>

    <boolean_false value="1" />

    <boolean_true value="0" />

    <general_exception_codigo value="ESPECIAL" />

    <have_multi_environment value="yes" />

    <outputfield_attr value="yes" />

    <req_num_param value="no" />

    <translator value="MC2PPTPG" />

    <status_throw_exception value="yes" />

    <joltpool_name value="wcorp.bprocess.preciosmulticanal.gastos.GastosBean" /> <!-- WTC y JDBC no lo usan -->

  </context>

  <input>

        <field
          java_name = "codCaiGastoNotarial"
          type = "String"
          size = "4"
          middle_name = "codigo10"
          middle_name_secondary = "calificacion"
        />

        <field
          java_name = "codIicGastoNotarial"
          type = "String"
          size = "8"
          middle_name = "codigo11"
          middle_name_secondary = "canalEnvioRef"
        />

        <field
          java_name = "codSistema"
          type = "String"
          size = "3"
          middle_name = "codSistema"
          middle_name_secondary = "campoVariable"
        />

        <field
          java_name = "codEvento"
          type = "String"
          size = "3"
          middle_name = "codEventoPrimNivel"
          middle_name_secondary = "cambioTrabajo"
        />

        <field
          java_name = "tipo"
          type = "char"
          size = "1"
          middle_name = "tipo"
          middle_name_secondary = "cantCuotasCancel"
        />

        <field
          java_name = "fecha"
          type = "Date"
          size = "8"
          middle_name = "fecha"
          middle_name_secondary = "canalContacto"
          middle_date_format = "ddMMyyyy"
        />

        <field
          java_name = "hora"
          type = "String"
          size = "6"
          middle_name = "hora"
          middle_name_secondary = "canalEnvioNor"
        />

  </input>

  <output>

  </output>

</service>
