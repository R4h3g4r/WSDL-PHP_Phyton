<?xml version = "1.0" encoding = "ISO-8859-1" ?>
<!-- 
**
 * Obtener los datos para realizar una reversa
 * <p>
 * Registro de versiones:<ul>
 * <li>1.0 01/06/2007 Gonzalo Silva P. - Version inicial
 * </ul>
 * <p>
 * Notas de la Fuente de Datos
 * Base de datos : pagosatm
 * DataSource: DSPagosATM
 */
-->

<service name = "sp_AtmPsObtenerDatosReversa">

  <context>
    <boolean_false value="1" />
    <boolean_true value="0" />
    <general_exception_codigo value="ESPECIAL" />
    <have_multi_environment value="no" />
    <outputfield_attr value="no" />
    <req_num_param value="no" />
    <translator value="RORPPTPG" />
    <campo_indica_cantidad value="" />
    <status_throw_exception value="yes" />
    <joltpool_name value="wcorp.serv.atm.pagodeservicios.ATMPagoDeServiciosBean" />

  </context>
  <input>
  		<field
          java_name = "numeroTarjeta"
          type = "String"
          size = "100"
          middle_name = "NumeroTarjeta"
        />
       <field
          java_name = "numeroTerminal"
          type = "String"
          size = "6"
          middle_name = "NumeroTerminal"
        /> 
       <field
          java_name = "numeroCuenta"
          type = "String"
          size = "100"
          middle_name = "NumeroCuenta"
        />
        <field
          java_name = "idPagoRedbanc"
          type = "String"
          size = "100"
          middle_name = "IdPagoRedbanc"
        />
  </input> 

  <output> 
		<struct struct_size="0" class_name="DatosTgClienteVO" var_name="listaDatosTgCliente"> 
				<field java_name = "fechaTg"	type = "String"  size = "100"  middle_name = "Fecha" />
  			<field java_name = "sessionId"  	type = "String"  size = "250"  middle_name = "SessionId"/>
        <field java_name = "ticketId"   	type = "String"  size = "30"   middle_name = "TicketId" /> 
        <field java_name = "montoTotal" 	type = "String"  size = "30"   middle_name = "MontoTotal"/>
        <field java_name = "papelId"    	type = "String"  size = "30"   middle_name = "PapelId"/>
        <field java_name = "serial"     	type = "String"  size = "250"  middle_name = "Serial" />
        <field java_name = "crc"        	type = "String"  size = "40"   middle_name = "Crc"/>
        <field java_name = "turnoId"    	type = "String"  size = "30"   middle_name = "TurnoId" />  
        <field java_name = "nemonicoCargo" 	type = "Integer" size = "30"   middle_name = "NemonicoCargo" /> 
        <field java_name = "numeroCuenta"	type = "String"  size = "100"  middle_name = "NumeroCuenta" />
        <field java_name = "nombreEmpresa"	type = "String"  size = "100"  middle_name = "NombreEmpresa" />        
  	</struct>
  </output>
</service>