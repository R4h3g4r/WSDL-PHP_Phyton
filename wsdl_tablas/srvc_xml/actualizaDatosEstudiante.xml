        />

        <field
          java_name = "fechaNacimientoEstudiante"
          type = "Date"
          size = "20"
          middle_name = "fecSolicitud"
          middle_date_format = "ddMMyyyy"
        />

        <field
          java_name = "emailEstudiante"
          type = "String"
          size = "200"
          middle_name = "email"
        />

  </input>

  <output>

        <field
          java_name = "resultado"
          type = "Integer"
          size = "1"
          sign = "no"
          integers = "1"
          decimals = "0"
