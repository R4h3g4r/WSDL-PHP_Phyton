<?xml version = "1.0" encoding = "ISO-8859-1" ?>

<service xmlns = "http://wcorp/util/bee/serviciosdedatos"
         xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation = "http://wcorp/util/bee/serviciosdedatos
                              XMLService.xsd"
         name = "sp_CU_actualiza_estudiante"><!-- JDBC: nombre stored procedure -->

  <context>

    <boolean_false value="1" />

    <boolean_true value="0" />

    <general_exception_codigo value="ESPECIAL" />

    <es_funcion value="select" />

    <struct_result acquire_value_by="position" />

  </context>

  <input>

        <field
