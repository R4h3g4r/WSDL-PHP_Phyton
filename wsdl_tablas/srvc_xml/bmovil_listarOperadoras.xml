<service name = "sp_operadoras">

  <context>
    <boolean_false value="1" />
    <boolean_true value="0" />
    <general_exception_codigo value="ESPECIAL" />
    <have_multi_environment value="no" />
    <outputfield_attr value="no" />
    <req_num_param value="no" />
    <translator value="RORPPTPG" />
    <campo_indica_cantidad value="" />
    <status_throw_exception value="yes" />
    <joltpool_name value="wcorp.serv.bancaMovil.ServiciosBancaMovilBean" />
  </context>
  
  <input/>
  <output>
  <struct struct_size="0" class_name="RegistroOperadorasDTO" var_name="operadoras"> 
	 <field
          java_name = "codigoOperadora"
          type = "Integer"
          size = "20"
          middle_name = "id_operador"
        />
	 <field
          java_name = "nombreOperadora"
          type = "String"
          size = "20"
          middle_name = "nombre_operador"
        />
  </struct>
  </output>

</service>


