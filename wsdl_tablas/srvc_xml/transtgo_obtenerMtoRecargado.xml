<service name = "sp_obtieneMontoRecargadoEnBip">

  <context>

    <boolean_false value="1" />

    <boolean_true value="0" />

    <general_exception_codigo value="ESPECIAL" />

    <have_multi_environment value="no" />

    <outputfield_attr value="no" />

    <req_num_param value="no" />

    <translator value="RORPPTPG" />

    <campo_indica_cantidad value="" />

    <status_throw_exception value="yes" />

    <joltpool_name value="wcorp.serv.transantiago.ServicioTarjetaBIPBean" />

  </context>
  <input>
  		<field
          java_name = "rut"
          type = "String"
          size = "10"
          middle_name = "rut"
        />
		<field
          java_name = "canal"
          type = "String"
          size = "5"
          middle_name = "canal"
        />
     <field
          java_name = "fechaContable"
          type = "String"
          size = "250"
          middle_name = "fechaContable"
        />
  </input>
  <output>   
	 <struct struct_size="0" class_name="RegistrosDTO" var_name="listaRar"> 
        <field
          java_name = "mto_ra"
          type = "Long"
          size = "250"
          middle_name = "mto_ra"
        /> 
  </struct>	
 </output> 
</service>

