<service name = "sp_AtmPsCambiarEstadoTrx">

   <context>
    <boolean_false value="1" />
    <boolean_true value="0" />
    <general_exception_codigo value="ESPECIAL" />
    <es_funcion value="output_param" />
    <struct_result acquire_value_by="name" />
  </context>
  <input>
  		<field
          java_name = "numeroTarjeta"
          type = "String"
          size = "100"
          middle_name = "NumeroTarjeta"
        />
       <field
          java_name = "numeroTerminal"
          type = "String"
          size = "6"
          middle_name = "NumeroTerminal"
        /> 
       <field
          java_name = "numeroCuenta"
          type = "String"
          size = "100"
          middle_name = "NumeroCuenta"
        />
        <field
          java_name = "idPagoRedbanc"
          type = "String"
          size = "100"
          middle_name = "IdPagoRedbanc"
        />
        <field
          java_name = "estado"
          type = "String"
          size = "8"
          middle_name = "Estado"
        />
        <field
          java_name = "sqlstatus"
          type = "Integer"
          size = "250"
          middle_name = "sqlstatus"
        />
  </input> 
  <output> 
	<field
          java_name = "sqlstatus"
          type = "Integer"
          size = "250"
          middle_name = "sqlstatus"
        />
  </output>
</service>