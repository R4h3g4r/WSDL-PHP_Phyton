<service name = "sp_ins_DetalleTransaction">

  <context>

    <boolean_false value="1" />

    <boolean_true value="0" />

    <general_exception_codigo value="ESPECIAL" />

    <have_multi_environment value="no" />

    <outputfield_attr value="no" />

    <req_num_param value="no" />

    <translator value="RORPPTPG" />

    <campo_indica_cantidad value="" />

    <status_throw_exception value="yes" />

    <joltpool_name value="wcorp.serv.recargacelulares.ServiciosRecargaCelularesBean" />

  </context>
  <input>
  		<field
          java_name = "idTransaccion"
          type = "Integer"
          size = "18"
          middle_name = "idTransaccion"
        />

     <field
          java_name = "nroCuenta"
          type = "String"
          size = "250"
          middle_name = "nroCuenta"
        />
     <field
          java_name = "tipoOperacion"
          type = "Integer"
          size = "250"
          middle_name = "tipoOperacion"
        />   
     <field
          java_name = "monto"
          type = "Integer"
          size = "250"
          middle_name = "monto"
        />      
     <field
          java_name = "nroCaja"
          type = "Integer"
          size = "250"
          middle_name = "nroCaja"
        />         
     <field
          java_name = "idEstado"
          type = "Integer"
          size = "250"
          middle_name = "idEstado"
        />    

  </input>
  <output/>   
