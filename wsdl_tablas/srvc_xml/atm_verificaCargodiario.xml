<?xml version = "1.0" encoding = "ISO-8859-1" ?>
<!-- 
**
 * Retornar el monto total diario para una cuenta cargado
 * 
 * <p>
 * Registro de versiones:<ul>
 * <li>1.0 01/06/2007 Gonzalo Silva P. - Version inicial
 * </ul>
 * <p>
 * Notas de la Fuente de Datos
 * Base de datos : pagosatm
 * DataSource: DSPagosATM
 */
-->
<service name = "sp_AtmPsVerificaCargoDiario">

  <context>
    <boolean_false value="1" />
    <boolean_true value="0" />
    <general_exception_codigo value="ESPECIAL" />
    <have_multi_environment value="no" />
    <outputfield_attr value="no" />
    <req_num_param value="no" />
    <translator value="RORPPTPG" />
    <campo_indica_cantidad value="" />
    <status_throw_exception value="yes" />
    <joltpool_name value="wcorp.serv.atm.pagodeservicios.ATMPagoDeServiciosBean" />

  </context>
  
  <input>
  	<field java_name = "numeroCuenta" 	type = "String" size = "20" middle_name = "NumeroCuenta"/>
  </input>
   
  <output> 
	<struct struct_size="0" class_name="RegistrosDatosTgClienteVO" var_name="listaDatosTgCliente"> 	
		<field java_name = "montoTotal" type = "String" size = "20" middle_name = "MontoTotal"/>
	</struct>
  </output>
</service>
