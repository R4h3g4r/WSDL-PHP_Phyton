<?xml version = "1.0" encoding = "ISO-8859-1" ?>
<!DOCTYPE service PUBLIC "-//BEE S.A.//DTD Transaction Definition//ES" "http://www.bee.cl/dtds/XMLService.dtd" >
<service name = "sp_ins_com">
  
    <context>
      <boolean_false value="1" />
      <boolean_true value="0" />
      <general_exception_codigo value="ESPECIAL" />
      <have_multi_environment value="no" />
      <outputfield_attr value="no" />
      <req_num_param value="no" />
      <translator value="RORPPTPG" />
      <campo_indica_cantidad value="" />
      <status_throw_exception value="yes" />
	  <es_funcion value="select" />
      <joltpool_name value="" />
   </context>
  

   <input>
      <field
         java_name = "numeroDeLaBoleta"
         type = "String"
         size = "15"
         middle_name = "vcNumeroBoleta"
      />
      <field
         java_name = "identificadorUnico"
         type = "String"
         size = "40"
         middle_name = "vcIdUnico"
      />
      <field
         java_name = "identificadoDelCliente"
         type = "String"
         size = "15"
         middle_name = "vcIdCliente"
      />
      <field
         java_name = "codigoDelServicio"
         type = "String"
         size = "15"
         middle_name = "vcCodigoServicio"
      />
      <field
         java_name = "codigoBiller"
         type = "String"
         size = "15"
         middle_name = "vcCodigoEmpresa"
      />
      <field
         java_name = "monto"
         type = "Double"
         size = "8"
         middle_name = "nMonto"
      />
      <field
         java_name = "fechaDeVencimiento"
         type = "Date"
         size = "4"
         middle_name = "sdFechaVencimiento"
      />
      <field
         java_name = "fechaDePago"
         type = "Date"
         size = "4"
         middle_name = "sdFechaPago"
      />
      <field
         java_name = "estado"
         type = "String"
         size = "3"
         middle_name = "cEstado"
      />
      	<field
	    java_name = "codigoSku"
	    type = "String"
	    size = "8"
	    middle_name = "com_sku"
	/>
         <field
	    java_name = "codigoEmpresaDelNegocio"
            type = "long"
            size = "4"	    
	    middle_name = "com_emp_gst"
         />
         <field
	    java_name = "codigoEmpresaPagadora"
            type = "long"
            size = "4"	    
	    middle_name = "com_emp_pgd"
         />
         <field
	    java_name = "codigoOficinaPagadora"
            type = "long"
            size = "4"	    
	    middle_name = "com_ofi_pgd"
         />
   </input>

   
   <output>
      <field
            java_name = "identificadorDelComprobante"
            type = "int"
            size = "4"
            middle_name = "numero"/>
         
   </output>
</service>
