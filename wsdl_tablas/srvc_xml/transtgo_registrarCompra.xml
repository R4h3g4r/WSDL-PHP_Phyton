<?xml version = "1.0" encoding = "ISO-8859-1" ?>
<!-- 
**
 * SP para registrar la compra de un #RA y 
 * registrar la relacion del cliente con una tarjeta bip si esta no existe
 * <p>
 * Registro de versiones:<ul>
 * <li>1.0 19/12/2006 Daniel Casanueva . Neoris Chile - Versión inicial
 * </ul>
 * <p>
 * Notas de la Fuente de Datos
 * Base de datos : transtgo
 * @version 1.0
 */
-->

<service name = "sp_registrar_compra">

  <context>

    <boolean_false value="1" />

    <boolean_true value="0" />

    <general_exception_codigo value="ESPECIAL" />

    <have_multi_environment value="no" />

    <outputfield_attr value="no" />

    <req_num_param value="no" />

    <translator value="RORPPTPG" />

    <campo_indica_cantidad value="" />

    <status_throw_exception value="yes" />

    <joltpool_name value="wcorp.serv.transantiago.ServicioTarjetaBIPBean" />

  </context>
  <input>
		
		<field
          java_name = "numeroTarjeta"
          type = "String"
          size = "10"
          middle_name = "numExternoTarjeta"
        />
		<field
          java_name = "rut"
          type = "String"
          size = "10"
          middle_name = "rutCliente"
        />
		<field
          java_name = "digitoVerificador"
          type = "char"
          size = "1"
          middle_name = "dvCliente"
        />
		<field
          java_name = "alias"
          type = "String"
          size = "25"
          middle_name = "codAlias"
        />
		<field
          java_name = "fechaOperacion"
          type = "String"
          size = "25"
          middle_name = "fechaCreacionRel"
        />
		<field
          java_name = "fechaContable"
          type = "String"
          size = "25"
          middle_name = "fecha_contable"
        />
		<field
          java_name = "estadoRelelacion"
          type = "String"
          size = "3"
          middle_name = "estadoRel"
        />
		<field
          java_name = "mail"
          type = "String"
          size = "50"
          middle_name = "mailAlias"
        />
  		<field
          java_name = "idTransaccion"
          type = "String"
          size = "24"
          middle_name = "idTransaccion"
        />
     <field
          java_name = "canal"
          type = "String"
          size = "5"
          middle_name = "canal"
        />   
     <field
          java_name = "monto"
          type = "String"
          size = "12"
          middle_name = "monto"
        />   
     <field
          java_name = "puntoAcceso"
          type = "String"
          size = "8"
          middle_name = "codigoPac"
        />      
     <field
          java_name = "numeroCuenta"
          type = "String"
          size = "12"
          middle_name = "ctaOrigen"
        />         
     <field
          java_name = "tipoCuenta"
          type = "String"
          size = "3"
          middle_name = "tipoCuentaOrigen"
        />    
     <field
          java_name = "idContexto"
          type = "String"
          size = "16"
          middle_name = "idContexto"
        />
     <field
          java_name = "numeroInterno"
          type = "String"
          size = "10"
          middle_name = "numInternoTarjeta"
        />
        <field
          java_name = "numeroRA"
          type = "String"
          size = "20"
          middle_name = "numeroRA"
        />      
  	
  </input>

  <output/>      

</service>
