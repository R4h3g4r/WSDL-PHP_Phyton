<service name = "sp_obtieneOperadoras">

  <context>

    <boolean_false value="1" />

    <boolean_true value="0" />

    <general_exception_codigo value="ESPECIAL" />

    <have_multi_environment value="no" />

    <outputfield_attr value="no" />

    <req_num_param value="no" />

    <translator value="RORPPTPG" />

    <campo_indica_cantidad value="" />

    <status_throw_exception value="yes" />

    <joltpool_name value="wcorp.serv.recargacelulares.ServiciosRecargaCelularesBean" />

  </context>
  <input/>

  <output>
  <struct struct_size="0" class_name="OperadoraVO" var_name="operadoras"> 
     <field
          java_name = "idOperadora"
          type = "Integer"
          size = "15"
          middle_name = "idOperadora"
        />

     <field
          java_name = "descripcionOperadora"
          type = "String"
          size = "250"
          middle_name = "descripcionOperadora"
        />
     <field
          java_name = "idTipoRango"
          type = "Integer"
          size = "250"
          middle_name = "idTipoRango"
        />   
     <field
          java_name = "descripcionTipoRango"
          type = "String"
          size = "250"
          middle_name = "descripcionTipoRango"
        />      
     <field
          java_name = "idRango"
          type = "Integer"
          size = "250"
          middle_name = "idRango"
        />         
     <field
          java_name = "valorInicial"
          type = "Integer"
          size = "250"
          middle_name = "valorInicial"
        />      
     <field
          java_name = "valorFinal"
          type = "Integer"
          size = "250"
          middle_name = "valorFinal"
        />   
  <field
          java_name = "tipoOperadora"
          type = "String"
          size = "20"
          middle_name = "tipo"
        />   
  </struct>
  </output>

</service>
