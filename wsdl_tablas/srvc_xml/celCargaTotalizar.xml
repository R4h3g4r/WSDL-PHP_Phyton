<service name = "sp_totalTransOperadoras">

  <context>

    <boolean_false value="1" />

    <boolean_true value="0" />

    <general_exception_codigo value="ESPECIAL" />

    <have_multi_environment value="no" />

    <outputfield_attr value="no" />

    <req_num_param value="no" />

    <translator value="RORPPTPG" />

    <campo_indica_cantidad value="" />

    <status_throw_exception value="yes" />

    <joltpool_name value="wcorp.serv.recargacelulares.ServiciosRecargaCelularesBean" />

  </context>
  <input/>  	  		 
  <output>   
  	    <struct struct_size="0" class_name="TotalOperadoraVO" var_name="totalizadores"> 
     <field
          java_name = "idOperadora"
          type = "Integer"
          size = "15"
          middle_name = "idOperadora"
        />

     <field
          java_name = "comision"
          type = "Integer"
          size = "250"
          middle_name = "comision"
        />
     <field
          java_name = "total"
          type = "Double"
          size = "250"
          middle_name = "total"
        />   
  </struct>
    </output>      

</service>
