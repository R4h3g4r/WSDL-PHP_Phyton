<service name = "sp_AtmPsActualizarEmpresasOnL">
	<context>
	    <boolean_false value="1" />
	    <boolean_true value="0" />
	    <general_exception_codigo value="ESPECIAL" />
	    <have_multi_environment value="no" />
	    <outputfield_attr value="no" />
	    <req_num_param value="no" />
	    <translator value="RORPPTPG" />
	    <campo_indica_cantidad value="" />
	    <status_throw_exception value="yes" />
		<joltpool_name value="wcorp.serv.atm.pagodeservicios.ATMPagoDeServiciosBean" />
	</context>
	<input>
		<field java_name = "codigoSesion" type = "String" size = "255" 
			   middle_name = "CodigoSesion"/>
		<field java_name = "codigoEmpresa" type = "Long" size = "7" 
			   middle_name = "CodigoEmpresa"/>
		<field java_name = "permiteReversaEnLinea" type = "String" size = "6" 
			   middle_name = "PermiteReversaEnLinea"/>
		<field java_name = "permiteRecaudacionParcial" type = "String" size = "6" 
			   middle_name = "PermiteRecaudacionParcial"/>
		<field java_name = "codigoModalidadServicio" type = "Integer" size = "2" 
			   middle_name = "CodigoModalidadServicio"/>
		<field java_name = "abreTagParametro" type = "String" size = "250" 
			   middle_name = "AbreTagParametro"/>
		<field java_name = "cierraTagParametro" type = "String" size = "250" 
			   middle_name = "CierraTagParametro"/>	   
		<field java_name = "tipoParametro" type = "String" size = "50" 
			   middle_name = "TipoParametro"/>	   	   
	</input>	
	<output> 
		
    </output>
</service>