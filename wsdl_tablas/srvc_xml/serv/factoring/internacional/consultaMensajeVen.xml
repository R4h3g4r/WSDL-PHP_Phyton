<?xml version = "1.0" encoding = "ISO-8859-1" ?>
<!DOCTYPE service PUBLIC "-//BEE S.A.//DTD Transaction Definition//ES" "http://www.bee.cl/dtds/XMLService.dtd" >
<service name = "sp_cns_men_csv">
   <context>
      <boolean_false value="1" />
      <boolean_true value="0" />
      <general_exception_codigo value="ESPECIAL" />
      <have_multi_environment value="no" />
      <outputfield_attr value="no" />
      <req_num_param value="no" />
      <translator value="RORPPTPG" />
      <campo_indica_cantidad value="" />
      <status_throw_exception value="yes" />
      <joltpool_name value="" />
   </context>

   <input>
      <field
          java_name = "vendedor"
          type = "String"
          size = "50"
          middle_name = "vendedor"
      />
      <field
          java_name = "fechaInicio"
          type = "Date"
          size = "50"
          middle_name = "fechaInicio"          
          middle_date_format = "ddMMyyyy"
      />
      <field
          java_name = "fechaFin"
          type = "Date"
          size = "50"
          middle_name = "fechaFin"          
          middle_date_format = "ddMMyyyy"
      />
      <field
          java_name = "tagXmlVendedor"
          type = "String"
          size = "100"
          middle_name = "tagXmlVendedor"
      />
      <field
          java_name = "tagXmlResponsable"
          type = "String"
          size = "100"
          middle_name = "tagXmlResponsable"
      />
      <field
          java_name = "tagXmlFecha"
          type = "String"
          size = "100"
          middle_name = "tagXmlFecha"
      />
   </input>
   <output>
   	<struct struct_size="0" class_name="ResultConsultaMensajeDTO" var_name="consultaMensajeDTO">
      <field
            java_name = "titulo"
            type = "String"
            size = "200"
            middle_name = "Prd_Des_msg"/>
      <field
            java_name = "identificador"
            type = "int"
            size = "10"
            middle_name = "Prd_Cod_req_enc"/>
            
      <field
            java_name = "codigo"
            type = "int"
            size = "10"
            middle_name = "Prd_Cod_enc_msg"/>
            
      <field
            java_name = "originador"
            type = "String"
            size = "150"
            middle_name = "Prd_Txt_val_atr"/>
     <field
            java_name = "indicadorEnvio"
            type = "int"
            size = "9"
            middle_name = "Prd_Txt_rvs"/>            
     <field
           java_name = "indicadorRespuesta"
           type = "int"
           size = "9"
           middle_name = "Prd_Txt_env"/>   
	 </struct>
   </output>
</service>
