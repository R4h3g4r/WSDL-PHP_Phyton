<?xml version = "1.0" encoding = "ISO-8859-1" ?>
<!DOCTYPE service PUBLIC "-//BEE S.A.//DTD Transaction Definition//ES" "http://www.bee.cl/dtds/XMLService.dtd" >
<service name = "sp_mod_est_vvi">
   <context>
      <es_funcion value="select"/>
   </context>

   <input>
      <field
         java_name = "numeroDeSistema"
         type = "String"
         size = "4"
         middle_name = "numeroDeSistema"
      />
      <field
         java_name = "correlativo"
         type = "String"
         size = "8"
         middle_name = "correlativo"
      />
      <field
         java_name = "codigoDeOficinaDeEntrega"
         type = "String"
         size = "3"
         middle_name = "codigoDeOficinaDeEntrega"
      />
      <field
         java_name = "codigoDeEjecutivo"
         type = "String"
         size = "12"
         middle_name = "codigoDeEjecutivo"
      />
	  <field
         java_name = "estado"
         type = "String"
         size = "2"
         middle_name = "estado"
      />
      
   </input>

   
   <output>
      <field
            java_name = "resultado"
            type = "int"
            size = "4"
            middle_name = "resultado"
         />
         
   </output>
</service>
