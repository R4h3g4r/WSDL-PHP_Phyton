<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- 
**
 * SP para ingresar un abono en cuentas corrientes.
 * <p>
 * Registro de versiones:<ul>
 * <li>1.0  28/02/2008  Judith Angulo Gómez - Version inicial
 * </ul>
 * <p>
 * Notas de la Fuente de Datos
 * Base de datos : cuentasmx
 * DataSource: ds/cuentasmx
 */
-->
<service name = "sp_ins_sol_abo">

  <context>

    <boolean_false value="1"/>

    <boolean_true value="0"/>

    <general_exception_codigo value="ESPECIAL"/>

    <have_multi_environment value="no"/>

    <outputfield_attr value="no"/>

    <req_num_param value="no"/>

    <translator value="RORPPTPG"/>

    <campo_indica_cantidad value=""/>

    <status_throw_exception value="yes"/>

    <joltpool_name value="wcorp.bprocess.mesadedinero.ruteadorpagos.DummyFacadeImpl" />

  </context>
   <input>
	  <field
         java_name = "origen"
         type = "String"
         size = "05"
         middle_name = "sol_ori"
	  />
	  <field
         java_name = "numeroOperacion"
         type = "String"
         size = "16"
         middle_name = "sol_num_ope"
      />
	  <field
         java_name = "rutBeneficiario"
         type = "String"
         size = "09"
         middle_name = "sol_rut_ben"
      />	
	  <field
         java_name = "digitoVerificadorBeneficiario"
         type = "String"
         size = "01"
         middle_name = "sol_dv_ben"
      />	
	  <field
         java_name = "cuentaCorrienteBeneficiario"
         type = "String"
         size = "16"
         middle_name = "sol_cta_cte_ben"
      />	
	  <field
         java_name = "nombreBeneficiario"
         type = "String"
         size = "50"
         middle_name = "sol_nom_ben"
      />	
	  <field
         java_name = "codigoMonedaTransaccion"
         type = "String"
         size = "05"
         middle_name = "mon_cod"
      />	
	  <field
         java_name = "montoTransaccion"
         type = "double"
         size = "250"
         middle_name = "sol_mto_trn"
      />
	  <field
         java_name = "nemonicoCartola"
         type = "String"
         size = "04"
         middle_name = "sol_mne_car"
      />
	   <field
         java_name = "fechaAbono"
         type = "String"
         size = "08"
         middle_name = "sol_fec_abo"
      />	
	  <field
         java_name = "horaAbono"
         type = "String"
         size = "05"
         middle_name = "hrs_abo"
      />
	  <field
         java_name = "bancoRemitente"
         type = "String"
         size = "35"
         middle_name = "sol_bco_rem"
      />
	  <field
         java_name = "rutOrdenante"
         type = "String"
         size = "09"
         middle_name = "sol_rut_ord"
      />
	  <field
         java_name = "digitoVerificadorOrdenante"
         type = "String"
         size = "01"
         middle_name = "sol_dv_ord"
      />
	  <field
         java_name = "cuentaCorrienteOrdenante"
         type = "String"
         size = "16"
         middle_name = "sol_cta_cte_ord"
      />
	  <field
         java_name = "nombreOrdenante"
         type = "String"
         size = "50"
         middle_name = "sol_nom_ord"
      />
	  <field
         java_name = "usuarioAplicacion"
         type = "String"
         size = "12"
         middle_name = "sol_usr_sol"
      />
     </input>
	 <output>	
	   	<struct struct_size="0" class_name="ResultadoIngresoPago" var_name="resultadoIngresoPago"> 
	   		<field
	            java_name = "indicadorResultadoOperacion"
	            type = "int"
				size = "10"
	            middle_name = "CODIGO"
	         />
	         <field
	            java_name = "mensajeResultadoOperacion"
	            type = "String"
	            size = "255"
	            middle_name = "DESCRIPCION"
	         />				  
		</struct> 
    </output> 	
</service>