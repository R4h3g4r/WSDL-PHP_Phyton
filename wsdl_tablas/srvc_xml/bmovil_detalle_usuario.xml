<service name = "sp_detalle_usuario">

  <context>
    <boolean_false value="1" />
    <boolean_true value="0" />
    <general_exception_codigo value="ESPECIAL" />
    <have_multi_environment value="no" />
    <outputfield_attr value="no" />
    <req_num_param value="no" />
    <translator value="RORPPTPG" />
    <campo_indica_cantidad value="" />
    <status_throw_exception value="yes" />
    <joltpool_name value="wcorp.serv.bancaMovil.ServiciosBancaMovilBean" />
  </context>
  
  <input>
  	<field 
		java_name = "rut"	
		type = "String"	
		size = "10"	
		middle_name = "rut_cliente"
	/>
  </input>
  <output>
  <struct struct_size="0" class_name="RegistroDatosUsuarioDTO" var_name="usuario">
	 <field
          java_name = "idUsuario"
          type = "Integer"
          size = "20"
          middle_name = "id_usuario"
        />
	 <field
          java_name = "rut"
          type = "String"
          size = "10"
          middle_name = "rut_usuario"
        />
     <field
          java_name = "numeroMovil"
          type = "Integer"
          size = "20"
          middle_name = "numero_movil"
        />
     <field
          java_name = "codigoActivacion"
          type = "Integer"
          size = "20"
          middle_name = "codigo_activacion"
        />
     <field
          java_name = "estadoUsuario"
          type = "String"
          size = "1"
          middle_name = "estado_usuario"
        />        
     <field
          java_name = "ultimaFecha"
          type = "String"
          size = "15"
          middle_name = "ultima_fecha"
        />
     <field
          java_name = "idBanco"
          type = "Integer"
          size = "24"
          middle_name = "id_marca"
        />
	 <field
          java_name = "nombreBanco"
          type = "String"
          size = "30"
          middle_name = "nombre_marca"
        />            
     <field
          java_name = "idOperadora"
          type = "Integer"
          size = "20"
          middle_name = "id_operador"
        />        
     <field
          java_name = "nombreOperadora"
          type = "String"
          size = "20"
          middle_name = "ope_nombre"
        />
	 <field
          java_name = "codigoModelo"
          type = "Integer"
          size = "24"
          middle_name = "id_modelo"
        />
	 <field
          java_name = "nombreModelo"
          type = "String"
          size = "50"
          middle_name = "nombre_modelo"
        />    
         <field
          java_name = "email"
          type = "String"
          size = "50"
          middle_name = "usu_email"
        />         
  </struct>
  </output>

</service>


