<?xml version = "1.0" encoding = "ISO-8859-1" ?>

<service xmlns = "http://wcorp/util/bee/serviciosdedatos"
         xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation = "http://wcorp/util/bee/serviciosdedatos
                              XMLService.xsd"
         name = "Mc2DesBloDatCon">

  <context>

    <boolean_false value="1" />

    <boolean_true value="0" />

    <general_exception_codigo value="ESPECIAL" />

    <have_multi_environment value="yes" />

    <outputfield_attr value="yes" />

    <req_num_param value="no" />

    <translator value="MC2PPTPG" />

    <status_throw_exception value="yes" />

    <joltpool_name value="wcorp.bprocess.preciosmulticanal.convenioscorporativos.ConveniosCorporativosBean" /> <!-- WTC y JDBC no lo usan -->

  </context>

  <input>

        <field
          java_name = "caiConvenio"
          type = "String"
          size = "4"
          middle_name = "idConvenio"
          middle_name_secondary = "tasaIntMax30"
        />

        <field
          java_name = "iicConvenio"
          type = "int"
          size = "8"
          sign = "no"
          integers = "8"
          decimals = "0"
          middle_name = "numConvenio"
          middle_name_secondary = "tasaIntMax48"
        />

        <field
          java_name = "fechaConvenio"
          type = "Date"
          size = "8"
          middle_name = "fecConstancia"
          middle_name_secondary = "tasaIntMax45"
          middle_date_format = "ddMMyyyy"
        />

        <field
          java_name = "horaConvenio"
          type = "String"
          size = "6"
          middle_name = "hora"
          middle_name_secondary = "tasaIntMax47"
        />

  </input>

  <output>

  </output>

</service>
