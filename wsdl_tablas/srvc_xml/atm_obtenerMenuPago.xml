<service name = "sp_AtmPsObtenerMenuPago">

  <context>

    <boolean_false value="1" />

    <boolean_true value="0" />

    <general_exception_codigo value="ESPECIAL" />

    <have_multi_environment value="no" />

    <outputfield_attr value="no" />

    <req_num_param value="no" />

    <translator value="RORPPTPG" />

    <campo_indica_cantidad value="" />

    <status_throw_exception value="yes" />

    <joltpool_name value="wcorp.serv.atm.pagodeservicios.ATMPagoDeServiciosBean" />

  </context>
  <input>
  		<field
          java_name = "codigoEmpresa"
          type = "Long"
          size = "7"
          middle_name = "CodigoEmpresa"
        />
  </input>

  <output>
  <struct struct_size="0" class_name="RegistrosVO" var_name="listaAtmPagoServiciosVO"> 
        
     <field
          java_name = "glosaDeContexto"
          type = "String"
          size = "32"
          middle_name = "GlosaDeContexto"
        />		         

  </struct>
  </output>     

</service>
