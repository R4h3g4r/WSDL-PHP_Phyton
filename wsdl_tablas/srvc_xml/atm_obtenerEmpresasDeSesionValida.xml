<?xml version = "1.0" encoding = "ISO-8859-1" ?>
<!--
/**
 * SP para obtener las empresas disponibles en una sesion diaria de Servipag
 * <p>
 * Registro de versiones:<ul>
 * <li>1.0 01/06/2007 Rodrigo Rodriguez H. - Version inicial
 * </ul>
 * <p>
 * Notas de la Fuente de Datos
 * Base de datos : pagosatm
 * DataSource: DSPagosATM
  */
-->

<service name = "sp_AtmPsSesionValida">
	<context>
	    <boolean_false value="1" />
	    <boolean_true value="0" />
	    <general_exception_codigo value="ESPECIAL" />
	    <have_multi_environment value="no" />
	    <outputfield_attr value="no" />
	    <req_num_param value="no" />
	    <translator value="RORPPTPG" />
	    <campo_indica_cantidad value="" />
	    <status_throw_exception value="yes" />
		<joltpool_name value="wcorp.serv.atm.pagodeservicios.ATMPagoDeServiciosBean" />
	</context>
	<input>
	</input>	
	<output> 
		<struct struct_size="0" class_name="RegistroEmpresaOnlineVO" var_name="empresasDisponibles"> 
			<field java_name = "codigoSesion" type = "String" size = "255" 
				   middle_name = "CodigoSesion"/>
			<field java_name = "codigoEmpresa" type = "Long" size = "7" 
				   middle_name = "CodigoEmpresa"/>
			<field java_name = "permiteReversaEnLinea" type = "String" size = "6" 
				   middle_name = "PermiteReversaEnLinea"/>
			<field java_name = "permiteRecaudacionParcial" type = "String" size = "6" 
				   middle_name = "PermiteRecaudacionParcial"/>
			<field java_name = "codigoModalidadServicio" type = "Integer" size = "2" 
				   middle_name = "CodigoModalidadServicio"/>
			<field java_name = "abreTagParametro" type = "String" size = "250" 
				   middle_name = "AbreTagParametro"/>
			<field java_name = "cierraTagParametro" type = "String" size = "250" 
				   middle_name = "CierraTagParametro"/>
			<field java_name = "tipoParametro" type = "String" size = "50" 
			   middle_name = "TipoParametro"/>		   
			<field java_name = "nombreEmpresa" type = "String" size = "50" 
				   middle_name = "EmpresaNombre"/>
			<field java_name = "codigoRubro" type = "Long" size = "7" 
				   middle_name = "CodigoRubro"/>
			<field java_name = "nombreRubro" type = "String" size = "50" 
				   middle_name = "Descripcion"/>
			<field java_name = "utilizaDigitoVerificador" type = "Integer" size = "2" 
				   middle_name = "UsaDigitoVerificador"/>
			<field java_name = "completaConCerosIdentificador" type = "Integer" size = "2" 
				   middle_name = "AnteponeCerosIdentificador"/>
			<field java_name = "largoIdentificador" type = "Integer" size = "4" 
				   middle_name = "LargoDeCampo"/>
			<field java_name = "codigoMetadatoFecha" type = "Long" size = "6" 
				   middle_name = "CodigoMetadatoFecha"/>
			<field java_name = "codigoFormatoFecha" type = "Integer" size = "3" 
				   middle_name = "CodigoFormatoFecha"/>
	   
		</struct>	   
    </output>
</service>