<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
    
<sqlMap namespace="sucpyme.procedimientosAlmacenados">

    <resultMap id="ResultadoConsultarVariableRut"	class="java.util.HashMap">
        <result property="rut"             javaType="String"    column="rut"     jdbcType="varchar"/>
        <result property="valor"           column="pre_tablon"   jdbcType="varchar"/>
    </resultMap>

    <procedure id="consultarVariableRut" parameterClass="java.util.HashMap" resultMap="ResultadoConsultarVariableRut">
        {CALL sp_ods_cns_var_rut(#rut_cliente#)}
    </procedure>

</sqlMap>
