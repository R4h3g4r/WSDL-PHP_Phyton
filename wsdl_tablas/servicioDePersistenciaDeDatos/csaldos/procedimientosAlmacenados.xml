<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE sqlMap          PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"          "http://ibatis.apache.org/dtd/sql-map-2.dtd">    <sqlMap namespace="csaldos.procedimientosAlmacenados">    <!-- INI SOLICITUD CERTIFICADO-->	<resultMap id="resultCertificado" class="java.util.HashMap" >        <result property="csd_rut_cli" 			column="csd_rut_cli" 		jdbcType="varchar"    	javaType="String" />		<result property="csd_dv_cli" 			column="csd_dv_cli" 		jdbcType="varchar"    	javaType="String" />        <result property="csd_fec_emision"  	column="csd_fec_emision" 	jdbcType="DATE" 		javaType="String" />		<result property="csd_nom_razon" 		column="csd_nom_razon" 		jdbcType="varchar"   	javaType="String" />        <result property="csd_per_sol"  		column="csd_per_sol"  		jdbcType="varchar" 		javaType="String" />		<result property="csd_estado_sol" 		column="csd_estado_sol" 	jdbcType="NUMERIC"   	javaType="String" />		<result property="csd_cod_eje" 			column="csd_cod_eje" 		jdbcType="varchar"   	javaType="String" />		<result property="csd_direccion_cli" 	column="csd_direccion_cli" 	jdbcType="varchar"   	javaType="String" />		<result property="csd_comuna_cli" 		column="csd_comuna_cli" 	jdbcType="varchar"   	javaType="String" />    </resultMap>		<procedure id="consultaSolicitud" parameterClass="java.util.HashMap" resultMap="resultCertificado">        {CALL sp_cns_csd_certificado_saldos()}    </procedure>		<procedure id="insertarSolicitud" parameterClass="java.util.HashMap" resultClass="int">        {CALL sp_ins_csd_certificado_saldos(#csd_rut_cli#,#csd_dv_cli#,#csd_fec_emision#,#csd_nom_razon#,#csd_per_sol#,#csd_estado_sol#,#csd_cod_eje#,#csd_direccion_cli#,#csd_comuna_cli#)}    </procedure>			<!-- CUENTA CORRIENTE -->	<resultMap id="resultCct" class="java.util.HashMap" >	    <result property="cct_rut_cli" 		column="cct_rut_cli" 		jdbcType="varchar"    	javaType="String" />        <result property="cct_per_sol"  	column="cct_per_sol"  		jdbcType="varchar" 		javaType="String" />		<result property="cct_ope" 			column="cct_ope" 			jdbcType="varchar"   	javaType="String" />        <result property="cct_tip_ope"  	column="cct_tip_ope"  		jdbcType="varchar" 		javaType="String" />		<result property="cct_desc_ofi" 	column="cct_desc_ofi" 		jdbcType="varchar"   	javaType="String" />		<result property="cct_cod_mda" 		column="cct_cod_mda" 		jdbcType="varchar"   	javaType="String" />		<result property="cct_sdo" 			column="cct_sdo" 			jdbcType="NUMERIC"   	javaType="String" />    </resultMap>		<procedure id="cuentaCorriente" parameterClass="java.util.HashMap" resultMap="resultCct">        {CALL sp_obt_cct_cuenta_corriente(#rut#,#fecha#)}    </procedure>		<!-- CUENTA DE LEASING -->	<resultMap id="resultLea" class="java.util.HashMap" >	    <result property="lea_rut_cli" 		column="lea_rut_cli" 		jdbcType="varchar"    	javaType="String" />        <result property="lea_per_sol"  	column="lea_per_sol"  		jdbcType="varchar" 		javaType="String" />		<result property="lea_ope" 			column="lea_ope" 			jdbcType="varchar"   	javaType="String" />        <result property="lea_tip_ope"  	column="lea_tip_ope"  		jdbcType="varchar" 		javaType="String" />		<result property="lea_desc_ofi" 	column="lea_desc_ofi" 		jdbcType="varchar"   	javaType="String" />		<result property="lea_cod_mda" 		column="lea_cod_mda" 		jdbcType="varchar"   	javaType="String" />		<result property="lea_sdo_sol" 		column="lea_sdo_sol" 		jdbcType="NUMERIC"   	javaType="String" />        <result property="lea_fec_ape"  	column="lea_fec_ape"  		jdbcType="DATE" 		javaType="String" />		<result property="lea_fec_prx_vct" 	column="lea_fec_prx_vct" 	jdbcType="DATE"   		javaType="String" />    </resultMap>		<procedure id="cuentaLeasing" parameterClass="java.util.HashMap" resultMap="resultLea">        {CALL sp_obt_lea_leasing(#rut#,#fecha#)}    </procedure>		<!-- CUENTA DE DEPOSITO A PLAZO -->	<resultMap id="resultDap" class="java.util.HashMap" >	    <result property="dap_rut_cli" 		column="dap_rut_cli" 		jdbcType="varchar"    	javaType="String" />        <result property="dap_per_sol"  	column="dap_per_sol"  		jdbcType="varchar" 		javaType="String" />		<result property="dap_ope" 			column="dap_ope" 			jdbcType="varchar"   	javaType="String" />        <result property="dap_tip_ope"  	column="dap_tip_ope"  		jdbcType="varchar" 		javaType="String" />		<result property="dap_desc_ofi" 	column="dap_desc_ofi" 		jdbcType="varchar"   	javaType="String" />		<result property="dap_cod_mda" 		column="dap_cod_mda" 		jdbcType="varchar"   	javaType="String" />		<result property="dap_mto" 			column="dap_mto" 			jdbcType="NUMERIC"   	javaType="String" />		<result property="dap_fec_sol"  	column="dap_fec_sol"  		jdbcType="DATE" 		javaType="String" />		<result property="dap_tasa_int" 	column="dap_tasa_int" 		jdbcType="NUMERIC"   	javaType="String" />		<result property="dap_mto_venc" 	column="dap_mto_venc" 		jdbcType="NUMERIC"   	javaType="String" />        <result property="dap_fec_vct"  	column="dap_fec_vct"  		jdbcType="DATE" 		javaType="String" />		    </resultMap>		<procedure id="cuentaDeposito" parameterClass="java.util.HashMap" resultMap="resultDap">        {CALL sp_obt_dap_deposito_plazo(#rut#,#fecha#)}    </procedure>		<!-- CUENTA DE AHORRO -->	<resultMap id="resultAho" class="java.util.HashMap" >	    <result property="cah_rut_cli" 			column="cah_rut_cli" 		jdbcType="varchar"    	javaType="String" />        <result property="cah_per_sol"  		column="cah_per_sol"  		jdbcType="varchar" 		javaType="String" />		<result property="cah_ope" 				column="cah_ope" 			jdbcType="varchar"   	javaType="String" />        <result property="cah_tip_ope"  		column="cah_tip_ope"  		jdbcType="varchar" 		javaType="String" />		<result property="cah_desc_ofi" 		column="cah_desc_ofi" 		jdbcType="varchar"   	javaType="String" />		<result property="cah_cod_mda" 			column="cah_cod_mda" 		jdbcType="varchar"   	javaType="String" />        <result property="cah_sdo" 				column="cah_sdo" 			jdbcType="NUMERIC"    	javaType="String" />        <result property="cah_fec_ape"  		column="cah_fec_ape"  		jdbcType="DATE" 		javaType="String" />    </resultMap>		<procedure id="cuentaAhorro" parameterClass="java.util.HashMap" resultMap="resultAho">        {CALL sp_obt_cah_cuenta_ahorro(#rut#,#fecha#)}    </procedure>		<!-- CUENTA DE COBRANZA EXTRANJERA -->	<resultMap id="resultComex" class="java.util.HashMap" >	    <result property="cmx_rut_cli" 		column="cmx_rut_cli" 		jdbcType="varchar"    	javaType="String" />        <result property="cmx_per_sol"  	column="cmx_per_sol"  		jdbcType="varchar" 		javaType="String" />		<result property="cmx_ope" 			column="cmx_ope" 			jdbcType="varchar"   	javaType="String" />        <result property="cmx_tip_ope"  	column="cmx_tip_ope"  		jdbcType="varchar" 		javaType="String" />		<result property="cmx_desc_ofi" 	column="cmx_desc_ofi" 		jdbcType="varchar"   	javaType="String" />		<result property="cmx_cod_mda" 		column="cmx_cod_mda" 		jdbcType="varchar"   	javaType="String" />        <result property="cmx_sdo" 			column="cmx_sdo" 			jdbcType="NUMERIC"    	javaType="String" />    </resultMap>		<procedure id="cuentaCobranza" parameterClass="java.util.HashMap" resultMap="resultComex">        {CALL sp_obt_cmx_cobranza_extranjera(#rut#,#fecha#)}    </procedure>		<!-- CUENTA DE GARANTIAS -->	<resultMap id="resultGar" class="java.util.HashMap" >	    <result property="grt_rut_cli" 				column="grt_rut_cli" 			jdbcType="varchar"    	javaType="String" />        <result property="grt_per_sol"  			column="grt_per_sol"  			jdbcType="varchar" 		javaType="String" />		<result property="grt_ope" 					column="grt_ope" 				jdbcType="varchar"   	javaType="String" />        <result property="grt_tip_ope"  			column="grt_tip_ope"  			jdbcType="varchar" 		javaType="String" />		<result property="grt_desc_ofi" 			column="grt_desc_ofi" 			jdbcType="varchar"   	javaType="String" />		<result property="grt_cod_mda" 				column="grt_cod_mda" 			jdbcType="varchar"   	javaType="String" />        <result property="grt_mto_sol" 				column="grt_mto_sol" 			jdbcType="NUMERIC"    	javaType="String" />        <result property="grt_fec_constitucion"  	column="grt_fec_constitucion"  	jdbcType="DATE" 		javaType="String" />    </resultMap>		<procedure id="cuentaGarantia" parameterClass="java.util.HashMap" resultMap="resultGar">        {CALL sp_obt_grt_garantia(#rut#,#fecha#)}    </procedure>		<!-- CUENTA DE VALORES DE CAMBIO -->	<resultMap id="resultVap" class="java.util.HashMap" >	    <result property="vlc_rut_cli" 			column="vlc_rut_cli" 	jdbcType="varchar"    	javaType="String" />        <result property="vlc_per_sol"  		column="vlc_per_sol"  	jdbcType="varchar" 		javaType="String" />		<result property="vlc_nom_mda" 			column="vlc_nom_mda" 	jdbcType="varchar"   	javaType="String" />        <result property="vlc_val_mda"  		column="vlc_val_mda"  	jdbcType="NUMERIC" 		javaType="String" />    </resultMap>		<procedure id="cuentaValor" parameterClass="java.util.HashMap" resultMap="resultVap">        {CALL sp_obt_vlc_valor_cambio(#rut#,#fecha#)}    </procedure>		<!-- CUENTA DE DEUDA POR PRESTAMO -->	<resultMap id="resultDd" class="java.util.HashMap" >	    <result property="deu_rut_cli" 		column="deu_rut_cli" 	jdbcType="varchar"    	javaType="String" />        <result property="deu_per_sol"  	column="deu_per_sol"  	jdbcType="varchar" 		javaType="String" />		<result property="deu_ope" 			column="deu_ope" 		jdbcType="varchar"   	javaType="String" />        <result property="deu_tip_ope"  	column="deu_tip_ope"  	jdbcType="varchar" 		javaType="String" />		<result property="deu_desc_ofi" 	column="deu_desc_ofi" 	jdbcType="varchar"   	javaType="String" />		<result property="deu_cod_mda" 		column="deu_cod_mda" 	jdbcType="varchar"   	javaType="String" />        <result property="deu_sdo_sol" 		column="deu_sdo_sol" 	jdbcType="NUMERIC"    	javaType="String" />        <result property="deu_fec_cur"  	column="deu_fec_cur"  	jdbcType="DATE" 		javaType="String" />		<result property="deu_fec_vct"  	column="deu_fec_vct"  	jdbcType="DATE" 		javaType="String" />		    </resultMap>		<procedure id="cuentaPrestamo" parameterClass="java.util.HashMap" resultMap="resultDd">        {CALL sp_obt_deu_deuda_prestamo(#rut#,#fecha#)}    </procedure>		<!-- FIN SOLICITUD CERTIFICADO-->	</sqlMap>