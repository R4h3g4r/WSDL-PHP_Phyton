<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="firmasclienteportal.procedimientosAlmancenados">
	
    <procedure id="obtenerDatosCliente"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_info_cliente_ctacte(#numeroCuenta#)}
    </procedure>
    
    <procedure id="obtenerAgrupacionFacultades"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_agrup_facultades(
      		<isNull property="numeroCuenta">NULL</isNull><isNotNull property="numeroCuenta">#numeroCuenta#</isNotNull>,
      		<isNull property="rutCliente">NULL</isNull><isNotNull property="rutCliente">#rutCliente#</isNotNull>,
      		<isNull property="rutApoderado">NULL</isNull><isNotNull property="rutApoderado">#rutApoderado#</isNotNull>
      		)
      }
    </procedure>
    
    <procedure id="obtenerFirmantesCtaCte"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_firmantes_cuentacorriente(
      		<isNull property="numeroCuenta">NULL</isNull><isNotNull property="numeroCuenta">#numeroCuenta#</isNotNull>,
      		<isNull property="rutCliente">NULL</isNull><isNotNull property="rutCliente">#rutCliente#</isNotNull>
      		)
      }
    </procedure>
    
    <procedure id="obtenerListadoPoderes"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_listado_poderes(
			<isNull property="numeroCuenta">NULL</isNull><isNotNull property="numeroCuenta">#numeroCuenta#</isNotNull>,
      		<isNull property="rutCliente">NULL</isNull><isNotNull property="rutCliente">#rutCliente#</isNotNull>,
      		<isNull property="tipoFacultad">NULL</isNull><isNotNull property="tipoFacultad">#tipoFacultad#</isNotNull>
		)}
    </procedure>
    
    <procedure id="obtenerFirmantesPorRut"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_firmantes_por_cliente(#rutCliente#)}
    </procedure>
    
    <procedure id="obtenerDatosClienteRut"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_info_cliente_rut(#rutCliente#)}
    </procedure>

    <procedure id="obtenerFirmantesPorFirmante"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_firmas_historicas(#rutFirmante#)}
    </procedure>
    
    <procedure id="obtenerDatosFirmanteRut"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_info_firmante(#rutFirmante#)}
    </procedure>
    
    <procedure id="obtenerFirmantesPorEjecutivo"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_apoderados_por_ejecutivo(#ejecutivo#, #rutEmpresa#)}
    </procedure>
    
    <procedure id="obtenerListadoPoderesCliente"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_listado_poderes_por_cliente(#rutCliente#, #tipoFacultad#)}
    </procedure>
    
    <procedure id="obtenerDatosFirmantesPorFirmante"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_firmantes_por_firmante(#rutFirmante#)}
    </procedure>
    
    <procedure id="obtenerListadoPoderesFirmante"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_listado_poderes_por_firmante(#rutFirmante#, #tipoFacultad#)}
    </procedure>
    
    <procedure id="obtenerFirmantes"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_firmantes(#numeroCuenta#)}
    </procedure>
    
    <procedure id="obtenerDatosFirmaPorCuenta"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_datos_firma_por_cuenta(#numeroCuenta#)}
    </procedure>
    
    <procedure id="obtenerApoderados"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_apoderados(#numeroCuenta#)}
    </procedure>
    
    <procedure id="obtenerPoderes"  parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
      {CALL sp_obtener_poderes(#numeroCuenta#)}
    </procedure>
</sqlMap>	
