<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
    
<sqlMap namespace="oauth.procedimientosAlmacenados">
	<!-- Mapping de store procedure sp_ins_authorization_code -->
	<parameterMap id="setInsAuthorization" class="java.util.HashMap">			
		<parameter property="client_id" jdbcType="VARCHAR" javaType="String" />			
		<parameter property="code" jdbcType="VARCHAR" javaType="String" />			
		<parameter property="data" jdbcType="VARCHAR" javaType="String" />			
	</parameterMap>
	<procedure id="sp_ins_authorization_code" parameterMap="setInsAuthorization" resultClass="int">
		{CALL sp_ins_authorization_code(?, ?, ?)}
	</procedure>

</sqlMap>    
