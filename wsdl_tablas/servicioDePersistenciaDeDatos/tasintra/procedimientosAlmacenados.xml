<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
    
<sqlMap namespace="tasintra.procedimientosAlmacenados">
	
	<!-- Inicio: Registrar solicitud de tasación de un prospecto hipotecario -->
    <procedure id="registrarSolicitudDeTasacion" parameterClass="java.util.HashMap" resultClass="int">
        {CALL sp_ins_solicitud_urbana_ch_ext(#tipo#,
                                             #for_pago_des#,
                                             #ofi_pago#,
                                             #ape_pat#,
                                             #ape_mat#,
                                             #nom_cli#,
                                             #rut_cli#,
                                             #digv_cli#,
                                             #cli_dom#,
                                             #fono_cli#,
                                             #obs_obj#,
                                             #nom_pro#,
                                             #rut_pro#,
                                             #digv_pro#,
                                             #tipo_bien#,
                                             #dir_pro#,
                                             #dir_des#,
                                             #comuna#,
                                             #region#,
                                             #ciudad#,
                                             #contacto#,
                                             #fono_contacto#,
                                             #rol#,
                                             #usr_ejecutivo#)}
    </procedure>
    <!-- Fin: Registrar solicitud de tasación de un prospecto hipotecario -->
	
</sqlMap>
