<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
	
<sqlMap namespace="solbloqtdd.procedimientosAlmacenados">	
    
    <procedure id="solBloqTDD" parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
        {CALL sp_sol_bloq_tdd(#rut_cli#,#cta_cte#,#pan#,#bloqueo#)}
    </procedure>


	
	<parameterMap id="setCnsInfoTDD" class="java.util.HashMap">
		<parameter property="rutCliente" 	jdbcType="CHAR"  javaType="String" />
		<parameter property="pan" 			jdbcType="CHAR"  javaType="String" />
	</parameterMap>
 
 	<procedure id="cnsInfoTDD" parameterMap="setCnsInfoTDD" resultClass="java.util.HashMap">
		{CALL sp_cns_info_tdd (? , ? )}
	</procedure>	
	
</sqlMap>	


