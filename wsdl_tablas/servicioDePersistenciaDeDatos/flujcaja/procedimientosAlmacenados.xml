<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="flujcaja.procedimientosAlmacenados">

	<procedure id="consultarOperacionManual" resultClass="java.util.HashMap"
		parameterClass="java.util.HashMap">
		{CALL
		sp_cns_operacion_manual(<isNotNull property="ope_cod_item">#ope_cod_item#</isNotNull><isNull property="ope_cod_item">NULL</isNull>,
								<isNotNull property="ope_cod_producto">#ope_cod_producto#</isNotNull><isNull property="ope_cod_producto">NULL</isNull>,
								#ope_fec_desde#,#ope_fec_hasta#,
								<isNotNull property="usu_cod_usuario">#usu_cod_usuario#</isNotNull><isNull property="usu_cod_usuario">NULL</isNull>,
								<isNotNull property="ope_cod_negocio">#ope_cod_negocio#</isNotNull><isNull property="ope_cod_negocio">NULL</isNull>)}
	</procedure>

	<procedure id="eliminarOperacionManual" resultClass="java.util.HashMap"
		parameterClass="java.util.HashMap">
		{CALL sp_del_operacion_manual(#ope_cod_item#)}
	</procedure>

	<procedure id="insertarOperacionManual" resultClass="java.util.HashMap"
		parameterClass="java.util.HashMap">
		{CALL
		sp_ins_operacion_manual(#ope_cod_item#,#ope_fec_origen#,#ope_des_solicitado#,#ope_mto_monto#,#ope_cod_moneda#,
		#ope_fec_vencimiento#,#ope_cod_tipo_flujo#,#ope_cod_producto#,#usu_cod_usuario#,#ope_cod_negocio#)}
	</procedure>

	<procedure id="consultarParametros" resultClass="java.util.HashMap"
		parameterClass="java.util.HashMap">
		{CALL sp_cns_parametros(#par_cod_grupo#,
								<isNotNull property="par_cod_parametro">#par_cod_parametro#</isNotNull><isNull property="par_cod_parametro">NULL</isNull>)}
	</procedure>
	
	<procedure id="consultarAllParametros" resultClass="java.util.HashMap"
		parameterClass="java.util.HashMap">
		{CALL sp_cns_all_parametros()}
	</procedure>
	
	<procedure id="insertarParametros" resultClass="java.util.HashMap"
		parameterClass="java.util.HashMap">
		{CALL sp_ins_parametros(#par_cod_grupo#,#par_cod_parametro#,#par_des_parametro#,#par_ind_vigencia#)}
	</procedure>
	
	
	<procedure id="eliminarParametro" resultClass="java.util.HashMap"
		parameterClass="java.util.HashMap">
		{CALL sp_del_parametro(#par_cod_grupo#,#par_cod_parametro#)}
	</procedure>
	
	<procedure id="insertarUsuario" resultClass="java.util.HashMap"
		parameterClass="java.util.HashMap">
		{CALL
		sp_ins_usuario(#usu_cod_usuario#,#usu_nom_usuario#,#usu_nom_usuario_ape_mat#,#usu_nom_usuario_ape_pat#,#usu_cod_unidad_negocio#,#usu_cod_usuario_ingresa#,
		#usu_fec_ingreso#,#usu_cod_usuario_actualiza#,#usu_fec_actualiza#,#usu_ind_vigencia#)}
	</procedure>
		<procedure id="consultarModulos" resultClass="java.util.HashMap">
		{CALL sp_cns_modulo()}
	</procedure>
	<procedure id="insertarModulo" resultClass="java.util.HashMap"
		parameterClass="java.util.HashMap">
		{CALL sp_ins_modulo(#mod_id_modulo#,#usu_cod_usuario#,#usm_ind_vigencia#,#usm_fec_inicio_modulo#,#usm_fec_fin_modulo#)}
	</procedure>
	<procedure id="consultarUsuarios" resultClass="java.util.HashMap">
		{CALL sp_cns_priv_usuarios()}
	</procedure>
		<procedure id="eliminarUsuario" resultClass="java.util.HashMap"
		parameterClass="java.util.HashMap">
		{CALL sp_del_usuario(#usu_cod_usuario#)} 	
	</procedure>
		<procedure id="editarModulo" resultClass="java.util.HashMap"
		parameterClass="java.util.HashMap">
		{CALL sp_upt_modulo(#vigencia#,#usu_cod_usuario#,#mod_id_modulo#,#usm_fec_fin_modulo#)}
	</procedure>
		<procedure id="editarUsuario" resultClass="java.util.HashMap"
		parameterClass="java.util.HashMap">
		{
		CALL sp_upt_usuario(
		#usu_cod_usuario#,
		#usu_nom_usuario#,
		#usu_nom_usuario_ape_pat#,
		#usu_nom_usuario_ape_mat#,
		#usu_cod_unidad_negocio#,
		#usu_cod_usuario_actualiza#,
		#usu_fec_actualiza#,
		#usu_ind_vigencia#
		)
		}
	</procedure>
</sqlMap>