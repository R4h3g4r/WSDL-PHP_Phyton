<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="oadb.procedimientosAlmacenados">
	<resultMap id="listadoLlamadasAcciones" class="java.util.HashMap">
		<result property="pkey" javaType="String" column="pkey" />
		<result property="type" javaType="String" column="type" />
		<result property="fechaLlamada" javaType="Date" column="createtime" />
		<result property="rut" javaType="String" column="RUT" />
		<result property="banca" javaType="String" column="Banca" />
		<result property="ejecutivo" javaType="String" column="Ejecutivo_LoginID" />
		<result property="ejecutivoEstado" javaType="String" column="Ejecutivo_Estado" />
		<result property="canal" javaType="String" column="Canal" />
