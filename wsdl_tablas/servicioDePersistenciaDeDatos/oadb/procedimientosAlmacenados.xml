		<result property="duracionLlamada" javaType="String" column="DURATION" />
		<result property="telefonoOrigen" javaType="String" column="ani" />
	</resultMap>

	<procedure id="sp_bus_acc_cli" parameterClass="java.util.HashMap" resultMap="listadoLlamadasAcciones">
		{CALL sp_busc_acc_cli(#rutCliente#,#dvCliente#,#fechaHasta#,#fechaDesde#,#esBusquedaInicial#,#tpoCanal#)}
