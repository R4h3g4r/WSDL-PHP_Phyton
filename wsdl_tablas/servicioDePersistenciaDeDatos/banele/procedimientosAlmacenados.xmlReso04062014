<!-- CARTOLAS ELECTRONICAS - INICIO -->
    <!-- Permite obtener las tarifas de las cartola electronicas unitarias y corporativas
         desde la tabla CPS, consulta el sp obtiene_tarifas_convenio de la base de  datos
         banele y recibe como  parametros  de entrada el numero de convenio y  codigo del
         producto (Prod08)
     -->
    <parameterMap id="setConsultarTarifasCartolasCPS" class="java.util.HashMap">
        <parameter property="cnv_num" jdbcType="VARCHAR" javaType="String" />
        <parameter property="pdt_cod" jdbcType="VARCHAR" javaType="String" />
        <parameter property="ser_cod" jdbcType="VARCHAR" javaType="String" />
    </parameterMap>
    <resultMap id="getConsultarTarifasCartolasCPS" class="java.util.HashMap">
