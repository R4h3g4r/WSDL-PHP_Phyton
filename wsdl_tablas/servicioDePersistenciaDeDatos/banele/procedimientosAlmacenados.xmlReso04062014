    <!-- Detalle Transferencia
        dbo.sp_DetalleTransferencia ( 
                              @emp_num_rut  char(12),      Rut empresa
                                      @cnv_num      char(10),      Convenio
                                      @num_cta      char(12),      Numero de la Cuenta
                                      @trf_est      varchar(100),  Estado Transferencia en encabezado
                                      @dtf_est      varchar(100),  Estado movimiento en detalle
                                      @dtf_bco_dst  char(3),       Banco destino
                                      @trf_num      varchar(100),  Numero de la Transferencia
                                      @trf_tip      varchar(100),  Tipo Transferencia
                                      @trf_fec_pag  char(8)        Fecha de Pago yyyyMMdd
                                  )
    -->
    <parameterMap class="java.util.HashMap" id="setDetalleTTFF">
        <parameter property="rutEmpresa" jdbcType="CHAR" javaType="String"/>
        <parameter property="numConvenio" jdbcType="CHAR" javaType="String"/>
        <parameter property="cuentaOrigen" jdbcType="CHAR" javaType="String"/>
        <parameter property="estadoTTFF" jdbcType="CHAR" javaType="String"/>
        <parameter property="estadoMovimiento" jdbcType="CHAR" javaType="String"/>
        <parameter property="bancoDestino" jdbcType="CHAR" javaType="String"/>
        <parameter property="numeroTTFF" jdbcType="CHAR" javaType="String"/>
        <parameter property="tipoTTFF" jdbcType="CHAR" javaType="String"/>
        <parameter property="fechaPago" jdbcType="CHAR" javaType="String"/>
    </parameterMap>
