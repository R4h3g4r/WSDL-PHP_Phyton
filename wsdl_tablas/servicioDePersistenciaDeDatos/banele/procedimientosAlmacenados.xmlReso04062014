        <result property="cnv_num" jdbcType="CHAR" javaType="String"/>
        <result property="emp_num_rut" jdbcType="CHAR" javaType="String"/>
        <result property="emp_vrt_rut" jdbcType="CHAR" javaType="String"/>
        <result property="dtf_cta_dst" jdbcType="CHAR" javaType="String"/>
        <result property="dtf_bco_dst" jdbcType="CHAR" javaType="String"/>
        <result property="dtf_mto" jdbcType="DECIMAL" javaType="double"/>
        <result property="dtf_est" jdbcType="CHAR" javaType="String"/>
        <result property="dtf_cod_est" jdbcType="CHAR" javaType="String"/>
        <result property="dtf_tip_pag" jdbcType="CHAR" javaType="String"/>
