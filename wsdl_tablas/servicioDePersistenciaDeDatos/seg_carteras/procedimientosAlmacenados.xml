<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
    
<sqlMap namespace="seg_carteras.procedimientosAlmacenados">
       
    



	<resultMap id="listadoConsultaEstadoClienteSSC" class="java.util.HashMap">
		<result property="seg_cli_etp_act" javaType="String" column="seg_cli_etp_act"/>
	</resultMap>

    <procedure id="consultaEstadoClienteSSC" parameterClass="java.util.HashMap" resultMap="listadoConsultaEstadoClienteSSC">
        {CALL sp_con_est_clte_ssc(#rut_cliente#,#dv_cliente#)}
    </procedure>

	<resultMap id="listadoClienteCOS" class="java.util.HashMap">
		<result property="proceso" javaType="String" column="seg_cli_det_proc"/>
		<result property="tipo" javaType="String" column="seg_cli_det_tipo"/>
		<result property="estrategia" javaType="String" column="seg_cli_vlr_cos"/>
	</resultMap>

    <procedure id="obtenerListadoClienteCOS" parameterClass="java.util.HashMap" resultMap="listadoClienteCOS">
        {CALL sp_cns_det_clte_cos(#rut_cliente#,#dv_cliente#)}
    </procedure>


</sqlMap>    