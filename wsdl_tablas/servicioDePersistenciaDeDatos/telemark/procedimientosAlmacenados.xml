<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap      
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"  "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="telemark.procedimientosAlmacenados">
	
	<parameterMap id="setGrabaSolicitudTelemark" class="java.util.HashMap">
		<parameter property="rutCliente"	 		jdbcType="CHAR" 	javaType="String" />
		<parameter property="dvCliente"		 		jdbcType="CHAR" 	javaType="String" />
		<parameter property="nombreCliente"		jdbcType="CHAR" 	javaType="String" />
		<parameter property="apPatCliente"		jdbcType="CHAR" 	javaType="String" />
		<parameter property="apMatCliente"	 	jdbcType="CHAR" 	javaType="String" />
		<parameter property="campana"	 				jdbcType="CHAR" 	javaType="String" />
		<parameter property="producto"	 			jdbcType="CHAR" 	javaType="String" />
		<parameter property="sucursal"				jdbcType="CHAR" 	javaType="String" />
		<parameter property="visacion"				jdbcType="CHAR" 	javaType="String" />
		<parameter property="telefono"				jdbcType="CHAR" 	javaType="String" />		
		<parameter property="mail"				    jdbcType="CHAR" 	javaType="String" />
	</parameterMap>

	<procedure id="grabaSolicitudTelemark" parameterMap="setGrabaSolicitudTelemark" resultClass="int">
		{ CALL sp_ing_solicitud_telemark( ?,?,?,?,?,?,?,?,?,?,? ) }
	</procedure>

</sqlMap>