<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"          "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="gestordenominas.procedimientosAlmacenados">
	<!-- Obtiene nominas -->	<parameterMap id="setNomina" class="java.util.HashMap">		<parameter property="id"				      jdbcType="CHAR"            javaType="java.lang.String"/>       	<parameter property="nombre"                  jdbcType="VARCHAR"         javaType="java.lang.String"/>       	<parameter property="ticket"                  jdbcType="VARCHAR"         javaType="java.lang.String"/>       	<parameter property="fechaSubida"             jdbcType="DATETIME"        javaType="java.util.Date"/>       	<parameter property="rutFicticio"             jdbcType="NUMERIC"         javaType="java.lang.Long"/>       	<parameter property="dvFicticio"              jdbcType="CHAR"            javaType="java.lang.String"/>       	<parameter property="adicionalPri"            jdbcType="CHAR"            javaType="java.lang.String"/>       	<parameter property="adicionalSec"            jdbcType="CHAR"            javaType="java.lang.String"/>       	<parameter property="codigoRelacion"          jdbcType="VARCHAR"         javaType="java.lang.String"/>       	<parameter property="fechaInicioVigencia"     jdbcType="DATE"            javaType="java.util.Date"/>       	<parameter property="fechaFinVigencia"        jdbcType="DATE"            javaType="java.util.Date"/>       	<parameter property="cantidadRegistros"       jdbcType="INTEGER"         javaType="java.lang.Integer"/>		<parameter property="codigoAccion"            jdbcType="CHAR"            javaType="java.lang.String"/>		<parameter property="usuarioIngreso"          jdbcType="VARCHAR"         javaType="java.lang.String"/>		<parameter property="usuarioAutoriza"         jdbcType="VARCHAR"         javaType="java.lang.String"/>       	<parameter property="codigoEstado"            jdbcType="CHAR"            javaType="java.lang.String"/>		<parameter property="codigoTipo"              jdbcType="CHAR"            javaType="java.lang.String"/>	</parameterMap>	<resultMap id="getNomina" class="java.util.HashMap"> 		<result property="id"				       column="nom_nomina_id"              javaType="java.lang.String"/>       	<result property="nombre"                  column="nom_nombre_nom"             javaType="java.lang.String"/>       	<result property="ticket"                  column="nom_ticket_cod"             javaType="java.lang.String"/>       	<result property="fechaSubida"             column="nom_subida_fec"             javaType="java.util.Date"/>       	<result property="rutFicticio"             column="nom_ficticio_rut"           javaType="java.lang.Long"/>       	<result property="dvFicticio"              column="nom_ficticio_dv"            javaType="java.lang.String"/>       	<result property="adicionalPri"            column="nom_ficticio_adicional_pri" javaType="java.lang.String"/>       	<result property="adicionalSec"            column="nom_ficticio_adicional_sec" javaType="java.lang.String"/>       	<result property="codigoRelacion"          column="nom_relacion_precio_cod"    javaType="java.lang.String"/>       	<result property="fechaInicioVigencia"     column="nom_ini_vig_fec"            javaType="java.util.Date"/>       	<result property="fechaFinVigencia"        column="nom_fin_vig_fec"            javaType="java.util.Date"/>       	<result property="cantidadRegistros"       column="nom_registros_cnt"          javaType="java.lang.Long"/>		<result property="codigoAccion"            column="nom_accion_cod"             javaType="java.lang.String"/>		<result property="usuarioIngreso"          column="nom_usu_ing_cod"            javaType="java.lang.String"/>		<result property="usuarioAutoriza"         column="nom_usu_aut_cod"            javaType="java.lang.String"/>       	<result property="codigoEstado"            column="par_codigo_id_est"          javaType="java.lang.String"/>		<result property="codigoTipo"              column="par_codigo_id_tip"          javaType="java.lang.String"/>     </resultMap>           	<procedure id="obtenerNominas" resultMap="getNomina">        { call sp_obtiene_nominas () }    </procedure>
	<!-- Ingresar nomina -->	<procedure id="ingresarNomina" parameterMap="setNomina" resultClass="int">        { call sp_ingreso_nomina (?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?) }    </procedure>			<!-- Eliminar nomina -->		<parameterMap id="setEliminaNomina" class="java.util.HashMap">		<parameter property="id"				      jdbcType="CHAR"            javaType="java.lang.String"/>	</parameterMap>			<procedure id="eliminarNomina" parameterMap="setEliminaNomina" resultClass="int">        { call sp_eliminar_nomina (?) }    </procedure>    	<!-- Modifica estado y ticket nomina -->	<parameterMap id="setModificaEstadoTicketNomina" class="java.util.HashMap">		<parameter property="id"				      jdbcType="CHAR"            javaType="java.lang.String"/>       	<parameter property="codigoEstado"            jdbcType="CHAR"            javaType="java.lang.String"/>       	<parameter property="ticket"                  jdbcType="VARCHAR"         javaType="java.lang.String"/>        <parameter property="usuarioAutoriza"         jdbcType="VARCHAR"         javaType="java.lang.String"/>       		</parameterMap>	      <procedure id="modificaEstadoTicketNomina" parameterMap="setModificaEstadoTicketNomina" resultClass="int">        { call sp_mod_estado_ticket_nomina (?,?,?,?) }    </procedure>
</sqlMap>    
