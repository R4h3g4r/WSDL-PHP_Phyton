    <parameterMap class="java.util.HashMap" id="parametrosCambiarEstadoSolicitud">

        <!-- PRIMERA CONSULTA (SELECT) -->
        <parameter property="nro_solicitud"  jdbcType="NUMERIC"  javaType="Long"/>

        <!-- SEGUNDA CONSULTA (INSERT) -->
