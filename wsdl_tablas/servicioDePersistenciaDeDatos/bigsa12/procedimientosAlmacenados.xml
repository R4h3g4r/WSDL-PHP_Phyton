<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap 
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="BIGSA12.procedimientosAlmacenados">	
	<resultMap id="multiproteccion" class="java.util.HashMap">
		<result property="rut" column="rut" />
		<result property="digitoVerificador" column="digitoVerificador" />
		<result property="nombre" column="nombre" />
		<result property="producto" column="producto" />
		<result property="codigo" column="codigo" />
		<result property="descripcion" column="descripcion" />
		<result property="fecha1" column="fecha1" />
		<result property="tipo" column="tipo" />
		<result property="tipoMateria" column="tipoMateria" />
		<result property="numPoliza" column="numPoliza" />
		<result property="indEndoso" column="indEndoso" />
		<result property="fecha2" column="fecha2" />
		<result property="fecha3" column="fecha3" />
		<result property="fecha4" column="fecha4" />
		<result property="prima" column="prima" />
		<result property="moneda" column="moneda" />
	</resultMap>
	<procedure id="obtenerMultiproteccion" resultMap="multiproteccion" parameterClass="java.util.HashMap">
		{CALL SP_CONSULTA_NEG(#rut#)}
	</procedure>
</sqlMap>
