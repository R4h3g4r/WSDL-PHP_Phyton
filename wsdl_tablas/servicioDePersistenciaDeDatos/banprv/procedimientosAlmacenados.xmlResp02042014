<!-- 
        sp_CEII_proyeccion_ingresos ( 
           @rut_proveedor       CHAR(12), /* Rut del Proveedor, obligatorio */ 
           @vrt_proveedor       CHAR(01), /* DV del Proveedor, obligatorio */ 
           @odp_fec_pag_desde   CHAR(10), /* Fecha de Pago desde, opcional */ 
           @odp_fec_pag_hasta   CHAR(10), /* Fecha de Pago hasta, opcional */ 
           @pagina              INT,      /* Pagina seleccionada por el usuario, obligatorio */ 
           @cantRegistros       INT       /* Cantidad de registros por pagina, obligatorio */   
    -->
    <procedure id="proyeccionPagosProveedor" parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
       {CALL sp_CEII_proyeccion_ingresos (#rut_proveedor#, #vrt_proveedor#, #odp_fec_pag_desde#, #odp_fec_pag_hasta#, #pagina#, #cantRegistros#)}
    </procedure>
    
    <!-- 
        sp_CEII_grupos_proveedor
        (
          @rut_proveedor           char(12), /* Rut del Proveedor, obligatorio */
          @rut_pagador             char(12), /* Rut del Pagador, opcional      */
          @pago_fecha_inicio       char(10),
          @pago_fecha_termino      char(10),
          @recepcion_fecha_inicio  char(10),
          @recepcion_fecha_termino char(10),
          @pagina_actual           int,      /* Pagina Actual, opcional        */
          @reg_pagina              int,      /* Cant.regs.a desplegar x pag, opcional */
          @nomina                  char(12), /* Nomina                         */
          @grupo                   char(7)   /* Grupo                          */
        )   
    -->
    <procedure id="gruposProveedor" parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
       {CALL sp_CEII_grupos_proveedor (#rut_proveedor#, #rut_pagador#, #pago_fecha_inicio#, #pago_fecha_termino#, 
                                  #recepcion_fecha_inicio#,#recepcion_fecha_termino#, #pagina_actual#, #reg_pagina#, 
                                  #nomina#, #grupo#)}
    </procedure>
    
    <!-- 
        sp_CEII_documento_proveedor
         (
          @rut_proveedor           char(12),
          @pagina_actual           int,
          @reg_pagina              int,
          @pago_fecha_inicio       char(10),
          @pago_fecha_termino      char(10),
          @recepcion_fecha_inicio  char(10),
          @recepcion_fecha_termino char(10),
          @rut_pagador             char(12),
          @tipo_documento          char(7),
          @numero_documento        char(25),
          @nomina                  char(12),
          @grupo                   char(7),
          @estado_documento        char(7)
        )   
    -->
    <procedure id="detalleDocumentoProveedor" parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
       {CALL sp_CEII_documento_proveedor (#rut_proveedor#, #pagina_actual#, #reg_pagina#, 
                                          #pago_fecha_inicio#, #pago_fecha_termino#, #recepcion_fecha_inicio#, 
                                          #recepcion_fecha_termino#, #rut_pagador#, #tipo_documento#, 
                                          #numero_documento#, #nomina#, #grupo#, 
                                          #estado_documento#)}
    </procedure>
    
    <!--
        sp_CEII_documento_proveedor
         (
          @rut_proveedor           char(12),
          @pagina_actual           int,
          @reg_pagina              int,
          @pago_fecha_inicio       char(10),
          @pago_fecha_termino      char(10),
          @recepcion_fecha_inicio  char(10),
          @recepcion_fecha_termino char(10),
          @rut_pagador             char(12),
          @tipo_documento          char(7),
          @numero_documento        char(25),
          @nomina                  char(12),
          @grupo                   char(7),
          @estado_documento        char(7)
        )
    -->
	<parameterMap id="setDocumentoProveedor" class="java.util.HashMap">
	    <parameter property="rut_proveedor"     		jdbcType="VARCHAR" javaType="String"/>
	    <parameter property="pagina_actual"	    		jdbcType="NUMERIC" javaType="int"/>
	    <parameter property="reg_pagina"	    		jdbcType="NUMERIC" javaType="int"/>
	    <parameter property="pago_fecha_inicio"			jdbcType="VARCHAR" javaType="String"/>
	    <parameter property="pago_fecha_termino"		jdbcType="VARCHAR" javaType="String"/>
	    <parameter property="recepcion_fecha_inicio"	jdbcType="VARCHAR" javaType="String"/>
	    <parameter property="recepcion_fecha_termino"	jdbcType="VARCHAR" javaType="String"/>
	    <parameter property="rut_pagador"				jdbcType="VARCHAR" javaType="String"/>
	    <parameter property="tipo_documento"			jdbcType="VARCHAR" javaType="String"/>
	    <parameter property="numero_documento"			jdbcType="VARCHAR" javaType="String"/>
	    <parameter property="nomina"					jdbcType="VARCHAR" javaType="String"/>
	    <parameter property="grupo"						jdbcType="VARCHAR" javaType="String"/>
	    <parameter property="estado_documento"			jdbcType="VARCHAR" javaType="String"/>
	</parameterMap>
	<resultMap id="getDocumentoProveedor" class="java.util.HashMap">
	  <result property="detalle_cliente"           column="detalle_cliente"           jdbcType="VARCHAR" javaType="String"     />
	  <result property="cantidad_registros"	       column="cantidad_registros"        jdbcType="NUMERIC" javaType="int"        />
