<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
	
<sqlMap namespace="resCreaProd.procedimientosAlmacenados">

    <parameterMap class="java.util.HashMap" id="setDatosResultadoOperacion">
        <parameter property="estado"                jdbcType="char"     javaType="String"/>
        <parameter property="inyeccion_detalle"     jdbcType="varchar"  javaType="String"/>
        <parameter property="producto"              jdbcType="varchar"  javaType="String"/>
        <parameter property="codigo_oportunidad"    jdbcType="int"      javaType="Integer"/>
        <parameter property="numero_producto"       jdbcType="char"     javaType="String"/>
        <parameter property="rut_cliente"           jdbcType="int"      javaType="Long"/>
        <parameter property="dv_cliente"            jdbcType="char"     javaType="String"/>
        <parameter property="inyeccion_fecha"       jdbcType="datetime" javaType="java.util.Date"/>
        <parameter property="ejecutivo"             jdbcType="char"     javaType="String"/>
    <parameter property="codigo_originador"     jdbcType="char"     javaType="String"/>
    <parameter property="ejecutivo_cierre"       jdbcType="char"     javaType="String"/>
    </parameterMap>
    
    <procedure id="insertarDatosResultadoOperacion" parameterMap="setDatosResultadoOperacion" >
        { CALL sp_ins_res_ope( ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?) }
    </procedure>
    
</sqlMap>
