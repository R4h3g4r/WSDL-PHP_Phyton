<?xml version="1.0" encoding="UTF-8"?>
<view id="consulta" types="ColCcc" maxHeight="440" maxWidth="850" numLoaded="5">
	<idDescription>consultaDeCreditos</idDescription>
	<header cols="6">
		<rows>
			<row>
				<field property="moneda" fieldType="TextType" enabled="false" width="150" type="string"/>
				<field property="tipoOperacion" fieldType="TextType" enabled="false" width="150" type="string"/>
				<field id="codigoAuxiliar" property="glsCodigoAuxiliar" fieldType="TextType" enabled="false" width="150" type="string"/>
			</row>
			<row>
				<field id="operacionAsignada" property="caiOperacion;iicOperacion" fieldType="AccountType" enabled="false"  width="150" />
				<field id="idCancelacion" property="caiCancelacion;iicCancelacion" fieldType="AccountType" enabled="false" width="150" />
				<field property="estado" fieldType="TextType" enabled="false" width="150" type="string"/>
			</row>
		</rows>
	</header>
	<tabs>
		<tab name="General" property="general" enabled="true" cols="6">
			<rows>
				<row>
					<field id="idDeudorDirecto" property="idDeudorDirecto;dvDeudorDirecto;indDeudorDirecto;glsDeudorDirecto" fieldType="RutType" enabled="false" width="150" type="rut"/>
					<field property="nombreDeudorDirecto" fieldType="TextType" enabled="false" width="150" type="string"/>
					<field property="canal" fieldType="TextType"  enabled="false" width="150" type="string"/>
					
					<field property="direccionAsociada" fieldType="ConstantType" type="string" visible="false"/>
					<field property="canalVenta" fieldType="ConstantType" type="string" visible="false"/>
				</row>
				<row>
					<field fieldType="HorizontalLine" colSpan="6"  />
				</row>
				<row>
					<field property="montoOperacion" fieldType="DecimalType" width="150" enabled="false" allowBlank="false"/>
					<field property="ejecutivo" fieldType="EjecutivoAdvancedComboBoxType" queryType="EJECUTIVOS" width="150" type="string" descriptionField="descCortaTabla" enabled="false" allowBlank="false" filterProperty="codigoDeTabla"/>
					<field property="fechaCurse" fieldType="DateType" enabled="false" width="150" allowBlank="false"/>
				</row>
				<row>
					<field fieldType="HorizontalLine" colSpan="6"  />
				</row>
				<row>
				<!-- ver xml -->
					<field id="tipoImpuesto" property="tipoImpuestos;valorImpuestos" fieldType="ComboNumberType" queryType="TIPO_IMPUESTO" width="150" enabled="false" allowBlank="false"/>
					<field property="condicionGarantia" queryType="CONDICION_GARANTIA" fieldType="ComboBoxType"  width="150" type="string" enabled="false" allowBlank="false"/>
					<field property="destino" fieldType="ComboBoxType" queryType="DESTINO_CREDITO" width="150"  type="string" enabled="false" allowBlank="false"/>
				</row>
				<row>
					<field property="tipoDePago"  fieldType="ComboBoxType" width="150" type="string" queryType="ABONOS_A_OPC" enabled="false" allowBlank="false"/>
					<field id="cuentaCobro" property="cuentaCobroCCC" fieldType="TextType" width="150" enabled="false" />
					<field property="vencimientosEstructurados" fieldType="YesNoType" width="150" type="bool" value="true" enabled="false" />
				</row>
				<row>
					<field id="tipoDeCargo" property="tipoCargo"  fieldType="ComboBoxType" width="150" type="string" queryType="TIPO_CARGOS_OPC"/>
					<field id="cuentaCargoLbl" property="cuentaCargo" fieldType="ComboBoxType" width="150" enabled="false" descriptionField="descCortaTabla"/>
					<field fieldType="LabelType" enabled="false" width="150"/>
				</row>
				<row>
					<field fieldType="HorizontalLine" colSpan="6"  />
				</row>
				<row>
					<field property="montoCredito" fieldType="DecimalType" enabled="false" width="150" value="0" type="integer"/>
					<field property="valorNeto" fieldType="DecimalType" enabled="false" width="150" value="0" type="integer"/>
					<field fieldType="LabelType" enabled="false" width="150"/>
				</row>
				<row>
					<field property="valorTasa" fieldType="DecimalType" enabled="false" width="150" value="0" type="integer"/>
					<field id="tipoTasa" property="descuentoTasa" fieldType="TextType" enabled="false" width="150"/>
					<field fieldType="LabelType" enabled="false" width="150"/>
				</row>
				<row>
					<field property="seguros" fieldType="DecimalType" enabled="false" width="150" value="0" type="integer"/>
					<field property="comisiones" fieldType="DecimalType" enabled="false" width="150" value="0" type="integer"/>
					<field property="gastos" fieldType="DecimalType" enabled="false" width="150" value="0" type="integer"/>
				</row>
				<row>
					<!-- valores heredados por tipo de plantilla -->
					<field property="modalidadCalculo" 	    fieldType="ConstantType" visible="false" value="IO" 	type="string"/>
					<field property="totalVencimientos2" 	fieldType="ConstantType" visible="false" value="1" 		type="integer"/>
					<field property="modalidadCalculo2" 	fieldType="ConstantType" visible="false" value="CO" 	type="string"/>
					<field property="codSegDesgrav" 		fieldType="ConstantType" visible="false" value="PRECIO" type="string"/>
				</row>
			</rows>
		</tab>
		<tab id="destinosSLG" property="destinosSLG" enabled="false"/>
		<tab property="multiplesdeudores" enabled="true"/>
		<tab id="proyectoInmobiliario" property="pry" enabled="false"/>
		<tab id="segurosClm" property="seguros" enabled="false"/>
		<tab id="comisiones" property="comisiones" enabled="false"/>
		<tab id="gastosClm" property="gastos" enabled="false"/>
		<tab property="intereses" enabled="false"/>
		<tab id="planDePagosClm" property="plc" enabled="false"/>
	</tabs>

</view>