<?xml version="1.0"?>
<!DOCTYPE jasperReport PUBLIC "-//JasperReports//DTD Report Design//EN" "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">

<jasperReport
		name="DataSourceReport"
		pageWidth="800"
		pageHeight="842"
		columnWidth="515"
		columnSpacing="0"
		leftMargin="40"
		rightMargin="40"
		topMargin="50"
		bottomMargin="50">
	<reportFont name="Arial_Normal" isDefault="true" fontName="Arial" size="12" pdfFontName="Helvetica" pdfEncoding="Cp1252" isPdfEmbedded="false"/>
	<reportFont name="Arial_Bold" isDefault="false" fontName="Arial" size="12" isBold="true" pdfFontName="Helvetica-Bold" pdfEncoding="Cp1252" isPdfEmbedded="false"/>
	<reportFont name="Arial_Italic" isDefault="false" fontName="Arial" size="12" isItalic="true" pdfFontName="Helvetica-Oblique" pdfEncoding="Cp1252" isPdfEmbedded="false"/>
	<parameter name="ReportTitle" class="java.lang.String"/>
    <parameter name="WebContext" class="java.lang.String"/>
	<field name="ClientING" class="java.lang.Float"/>
	<field name="ClientSOL" class="java.lang.Float"/>
	<field name="ClientCER" class="java.lang.Float"/>
        <field name="NoClientING" class="java.lang.Float"/>
	<field name="NoClientSOL" class="java.lang.Float"/>
	<field name="NoClientCER" class="java.lang.Float"/>
	<field name="Code" class="java.lang.String"/>
  	<field name="StrDescripcion" class="java.lang.String"/>
       	<variable name="ClientINGSumColumn" class="java.lang.Float" resetType="Page" calculation="Sum">
		<variableExpression>
			$F{ClientING}
		</variableExpression>
	</variable>
	<variable name="ClientSOLSumColumn" class="java.lang.Float" resetType="Page" calculation="Sum">
		<variableExpression>
			$F{ClientSOL}
		</variableExpression>
	</variable>
	<variable name="ClientCERSumColumn" class="java.lang.Float" resetType="Page" calculation="Sum">
		<variableExpression>
			$F{ClientCER}
		</variableExpression>
	</variable>
	<variable name="NoClientINGSumColumn" class="java.lang.Float" resetType="Page" calculation="Sum">
		<variableExpression>
			$F{NoClientING}
		</variableExpression>
	</variable>
	<variable name="NoClientSOLSumColumn" class="java.lang.Float" resetType="Page" calculation="Sum">
		<variableExpression>
			$F{NoClientSOL}
		</variableExpression>
	</variable>
	<variable name="NoClientCERSumColumn" class="java.lang.Float" resetType="Page" calculation="Sum">
		<variableExpression>
			$F{NoClientCER}
		</variableExpression>
	</variable>

	<title>
		<band height="70">
			<line>
				<reportElement x="0" y="0" width="800" height="0" forecolor="#000000"/>
				<graphicElement stretchType="NoStretch"/>
			</line>
			<textField isBlankWhenNull="true">
				<reportElement x="0" y="10" width="800" height="30"/>
				<textElement textAlignment="Center" lineSpacing="Single">
					<font reportFont="Arial_Normal" size="22"/>
				</textElement>
				<textFieldExpression class="java.lang.String">$P{ReportTitle}</textFieldExpression>
			</textField>
		</band>
	</title>
	<pageHeader>
		<band height="40">
			<staticText>
				<reportElement x="0" y="0" width="100" height="20" forecolor="#FFFFFF" backcolor="#333333" mode="Opaque"/>
				<textElement textAlignment="Center">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text></text>
			</staticText>
			<staticText>
				<reportElement x="100" y="0" width="210" height="20" forecolor="#FFFFFF" backcolor="#A0A0A0" mode="Opaque"/>
				<textElement textAlignment="Center">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>CLIENTS</text>
			</staticText>
			<staticText>
				<reportElement x="310" y="0" width="210" height="20" forecolor="#FFFFFF" backcolor="#A0A0A0" mode="Opaque"/>
				<textElement textAlignment="Center" >
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>NO CLIENTS</text>
			</staticText>


			<staticText>
				<reportElement x="0" y="20" width="100" height="20" forecolor="#FFFFFF" backcolor="#333333" mode="Opaque" />
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text></text>
			</staticText>

			<staticText>
				<reportElement x="100" y="20" width="70" height="20" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Ingresados</text>
			</staticText>
			<staticText>
				<reportElement x="170" y="20" width="70" height="20" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Solucionados</text>
			</staticText>
			<staticText>
				<reportElement x="240" y="20" width="70" height="20" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Cerrados</text>
			</staticText>
			<staticText>
				<reportElement x="310" y="20" width="70" height="20" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Ingresados</text>
			</staticText>
			<staticText>
				<reportElement x="380" y="20" width="70" height="20" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Solucionados</text>
			</staticText>
			<staticText>
				<reportElement x="450" y="20" width="70" height="20" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Cerrados</text>
			</staticText>

		</band>
	</pageHeader>
	<detail>
		<band height="20">
			<textField hyperlinkType="Reference" >
			<reportElement x="0" y="5" width="100" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Center">
				<font isUnderline="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					$F{StrDescripcion}
				</textFieldExpression>
				<hyperlinkReferenceExpression>$P{WebContext} + "ReportServlet?reportName=Resumen&#32;por&#32;tipo&#40;Reclamos&#41;&#38;typeCode="+$F{Code}</hyperlinkReferenceExpression>
			</textField>
			<textField pattern="0">
				<reportElement x="100" y="5" width="70" height="15"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$F{ClientING}
				</textFieldExpression>
			</textField>
			<textField pattern="0">
				<reportElement x="170" y="5" width="70" height="15"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$F{ClientSOL}
				</textFieldExpression>
			</textField>
			<textField pattern="0">
				<reportElement x="240" y="5" width="70" height="15" />
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$F{ClientCER}
				</textFieldExpression>
			</textField>
			<textField pattern="0">
				<reportElement x="310" y="5" width="70" height="15"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$F{NoClientING}
				</textFieldExpression>
			</textField>
			<textField pattern="0">
				<reportElement x="380" y="5" width="70" height="15"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$F{NoClientSOL}
				</textFieldExpression>
			</textField>
			<textField pattern="0">
				<reportElement x="450" y="5" width="70" height="15" />
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$F{NoClientCER}
				</textFieldExpression>
			</textField>
		</band>
	</detail>

	
	<summary>
		<band height="20">
			<staticText>
				<reportElement x="0" y="5" width="100" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Center" lineSpacing="Single">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Total </text>
			</staticText>

			<textField pattern="0">
				<reportElement x="100" y="5" width="70" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$V{ClientINGSumColumn}
				</textFieldExpression>
			</textField>
			<textField pattern="0" >
				<reportElement x="170" y="5" width="70" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$V{ClientSOLSumColumn}
				</textFieldExpression>
			</textField>
			<textField pattern="0" >
				<reportElement x="240" y="5" width="70" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$V{ClientCERSumColumn}
				</textFieldExpression>
			</textField>
			<textField pattern="0" >
				<reportElement x="310" y="5" width="70" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$V{NoClientINGSumColumn}
				</textFieldExpression>
			</textField>
			<textField pattern="0" >
				<reportElement x="380" y="5" width="70" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$V{NoClientSOLSumColumn}
				</textFieldExpression>
			</textField>
			<textField pattern="0" >
				<reportElement x="450" y="5" width="70" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$V{NoClientCERSumColumn}
				</textFieldExpression>
			</textField>

		</band>
	</summary>
</jasperReport>
