<?xml version="1.0"?>
<!DOCTYPE jasperReport PUBLIC "-//JasperReports//DTD Report Design//EN" "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">

<jasperReport
		name="DataSourceReport"
		pageWidth="1100"
		pageHeight="842"
		columnWidth="515"
		columnSpacing="0"
		leftMargin="40"
		rightMargin="40"
		topMargin="50"
		bottomMargin="50">
	<reportFont name="Arial_Normal" isDefault="true" fontName="Arial" size="12" pdfFontName="Helvetica" pdfEncoding="Cp1252" isPdfEmbedded="false"/>
	<reportFont name="Arial_Bold" isDefault="false" fontName="Arial" size="12" isBold="true" pdfFontName="Helvetica-Bold" pdfEncoding="Cp1252" isPdfEmbedded="false"/>
	<reportFont name="Arial_Italic" isDefault="false" fontName="Arial" size="12" isItalic="true" pdfFontName="Helvetica-Oblique" pdfEncoding="Cp1252" isPdfEmbedded="false"/>
	<parameter name="ReportTitle" class="java.lang.String"/>
    <parameter name="WebContext" class="java.lang.String"/>
	<field name="FltEventCode" class="java.lang.Float"/>
	<field name="StrCustRUT" class="java.lang.String"/>
	<field name="StrRefDescription" class="java.lang.String"/>
    <field name="StrEnterDate" class="java.lang.String"/>
	<field name="StrPromiseDate" class="java.lang.String"/>
	<field name="StrResolutionTime" class="java.lang.String"/>
	<field name="StrClosingDate" class="java.lang.String"/>
	<field name="StrAreaSource" class="java.lang.String"/>
	<field name="strStatusType" class="java.lang.String"/>
	<field name="strExecEnteredTask" class="java.lang.String"/>
	<title>
		<band height="70">
			<textField isBlankWhenNull="true">
				<reportElement x="0" y="10" width="1100" height="30"/>
				<textElement textAlignment="Center" lineSpacing="Single">
					<font reportFont="Arial_Normal" size="22"/>
				</textElement>
				<textFieldExpression class="java.lang.String">$P{ReportTitle}</textFieldExpression>
			</textField>
		</band>
	</title>
	<pageHeader>
		<band height="20">
			<staticText>
				<reportElement x="0" y="0" width="100" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>N2 ID</text>
			</staticText>
			<staticText>
				<reportElement x="100" y="0" width="100" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Rut</text>
			</staticText>
			<staticText>
				<reportElement x="200" y="0" width="100" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Referencia</text>
			</staticText>
			<staticText>
				<reportElement x="300" y="0" width="100" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Ejecutivo</text>
			</staticText>
			<staticText>
				<reportElement x="500" y="0" width="100" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Promise Date</text>
			</staticText>
			<staticText>
				<reportElement x="600" y="0" width="100" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Closing Date</text>
			</staticText>
			<staticText>
				<reportElement x="700" y="0" width="100" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Resolution Date</text>
			</staticText>
			<staticText>
				<reportElement x="800" y="0" width="100" height="15" backcolor="#C0C0C0" mode="Opaque"/>
				<textElement textAlignment="Right">
					<font reportFont="Arial_Normal"/>
				</textElement>
				<text>Area Source</text>
			</staticText>
		</band>
	</pageHeader>
	<detail>
		<band height="20">
			<textField >
			<reportElement x="0" y="5" width="100" height="15"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Float">
					$F{FltEventCode}
				</textFieldExpression>
    		</textField>
			<textField>
				<reportElement x="100" y="5" width="100" height="15"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.String">
					$F{StrCustRUT}
				</textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement x="200" y="5" width="100" height="15"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.String">
					$F{StrRefDescription}
				</textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement x="300" y="5" width="100" height="15" />
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.String">
					$F{strExecEnteredTask}
				</textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement x="500" y="5" width="100" height="15"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.String">
					$F{StrPromiseDate}
				</textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement x="600" y="5" width="100" height="15" />
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.String">
					$F{StrClosingDate}
				</textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement x="700" y="5" width="100" height="15" />
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.String">
					$F{StrResolutionTime}
				</textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement x="800" y="5" width="100" height="15" />
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.String">
					$F{StrAreaSource}
				</textFieldExpression>
			</textField>

		</band>
	</detail>
</jasperReport>
