<?xml version="1.0" encoding="iso-8859-1"?>
<!-- <== version: 2011/07/18 12:00 ==>-->
<xsl:stylesheet version="1.1" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:fo="http://www.w3.org/1999/XSL/Format" exclude-result-prefixes="fo">
    <xsl:output method="xml" version="1.0" omit-xml-declaration="no" indent="yes"/>
    <xsl:decimal-format name="decimal" decimal-separator=',' grouping-separator='.' />
    <xsl:decimal-format name="rut" grouping-separator='.' />
    <!-- <xsl:param name="IMG_URL"/>-->
    <xsl:template match="impresionBoletaGarantia">
        <fo:root xmlns:fo="http://www.w3.org/1999/XSL/Format">
        
            <fo:layout-master-set>
                <fo:simple-page-master master-name="cotizacion" page-height="297mm" page-width="215.9mm" margin-top="10mm" margin-bottom="10mm"  margin-right="25mm" margin-left="20mm">
                    <fo:region-before region-name="xsl-region-before" display-align="after" extent="15mm"/>
                    <fo:region-after region-name="xsl-region-after" display-align="before" extent="15mm"/>
                    <fo:region-body margin-top="10mm" margin-bottom="15mm"/>
                </fo:simple-page-master>
            </fo:layout-master-set>
            
            <fo:page-sequence master-reference="cotizacion" font-size="9pt" font-family="Arial, Helvetica, sans-serif" force-page-count="no-force" initial-page-number="1">
                <fo:flow flow-name="xsl-region-body" font-size="9pt" font-family="Arial, Helvetica, sans-serif">
                    <xsl:call-template name="bodyCert"/>
                    <fo:block id="theEndBanco"/>
                </fo:flow>
            </fo:page-sequence>
            
        </fo:root>
    </xsl:template>
    
    <xsl:template name="bodyCert">
            <fo:block>
                <fo:external-graphic vertical-align="middle" height="25mm" width="55mm">
                    <xsl:attribute name="src">
                        <xsl:value-of select="rutaImagen"/>
                    </xsl:attribute>
                </fo:external-graphic> 
            </fo:block>
            <fo:block>
                <fo:table>
                    <fo:table-column column-width="proportional-column-width(100)"/>
                    <fo:table-body>
                        <fo:table-row >
                            <fo:table-cell text-align="center" 
                                           border-top-style="solid" border-top-width="0px"
                                           border-bottom-style="solid" border-bottom-width="0px"
                                           border-left-style="solid" border-left-width="0px"
                                           border-right-style="solid" border-right-width="0px">
                                <fo:block>

                                    <fo:table table-layout="fixed" width="100%" border-collapse="collapse">
                                        <fo:table-column column-width="proportional-column-width(100)" column-number="1"/>
                                        <fo:table-body>
                                            <fo:table-row>
                                                <fo:table-cell>
                                                <fo:block></fo:block>
                                                    <fo:block text-decoration="underline" font-style="oblique" font-weight="bold" font-size="28pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="center" padding-top="10mm">
                                                        CERTIFICADO
                                                    </fo:block>
                                                </fo:table-cell>
                                            </fo:table-row>                                    
                                        </fo:table-body>
                                    </fo:table>                                        

                                    <fo:table table-layout="fixed" width="100%" border-collapse="collapse">
                                        <fo:table-column column-width="proportional-column-width(100)" column-number="1"/>
                                        <fo:table-body>
                                            <fo:table-row>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="justify" padding-top="10mm">
                                                        A trav&#xE9;s del presente Banco de Cr&#xE9;dito e Inversiones, Oficina Central, deja constancia 
                                                        que la Empresa <xsl:value-of select="nombreEmpresa"/>, RUT:<xsl:value-of select="rutEmpresa"/> mantiene 
                                                        vigente a la fecha la siguiente boleta de garant&#xED;a: 
                                                    </fo:block>
                                                </fo:table-cell>
                                            </fo:table-row>                                    
                                        </fo:table-body>
                                    </fo:table> 
                                    
                                    <fo:table table-layout="fixed" width="100%" border-collapse="collapse" padding-top="5mm">
                                        <fo:table-column column-width="proportional-column-width(40)" column-number="1"/>
                                        <fo:table-column column-width="proportional-column-width(10)" column-number="2"/>
                                        <fo:table-column column-width="proportional-column-width(50)" column-number="3"/>
                                        <fo:table-body>
                                            <fo:table-row>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         N&#xFA;mero Especie Valorada:
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         :
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         <xsl:value-of select="especie"/>
                                                    </fo:block>
                                                </fo:table-cell>
                                            </fo:table-row>     
                                            <fo:table-row>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         N&#xFA;mero Operaci&#xF3;n
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         :
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         <xsl:value-of select="operacion"/>
                                                    </fo:block>
                                                </fo:table-cell>
                                            </fo:table-row>                                    
                                            <fo:table-row>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         Moneda
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         :
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         <xsl:value-of select="moneda"/>
                                                    </fo:block>
                                                </fo:table-cell>
                                            </fo:table-row>                                    
                                            <fo:table-row>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         Monto
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         :
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         <xsl:value-of select="monto"/>
                                                    </fo:block>
                                                </fo:table-cell>
                                            </fo:table-row>                                    
                                            <fo:table-row>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         Beneficiario
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         :
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         <xsl:value-of select="benef"/>
                                                    </fo:block>
                                                </fo:table-cell>
                                            </fo:table-row>                                    
                                            <fo:table-row>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         Fecha de Emisi&#xF3;n
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         :
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         <xsl:value-of select="fechaEmi"/>
                                                    </fo:block>
                                                </fo:table-cell>
                                            </fo:table-row>                                    
                                            <fo:table-row>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         Fecha de Vencimiento
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         :
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         <xsl:value-of select="fechaVen"/>
                                                    </fo:block>
                                                </fo:table-cell>
                                            </fo:table-row>                                    
                                            <fo:table-row>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left" border-bottom-style="solid" border-bottom-width="0px">
                                                         Glosa
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left">
                                                         :
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="10pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="justify" border-bottom-style="solid" border-bottom-width="0px">
                                                         <xsl:value-of select="glosa"/>
                                                    </fo:block>
                                                </fo:table-cell>
                                            </fo:table-row>                                    
                                        </fo:table-body>
                                    </fo:table> 

                                    <fo:table table-layout="fixed" width="100%" border-collapse="collapse">
                                        <fo:table-column column-width="proportional-column-width(100)" column-number="1"/>
                                        <fo:table-body>
                                            <fo:table-row>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="justify" padding-top="5mm">
                                                    	Se extiende el presente certificado a petici&#xF3;n expresa del interesado para ser utilizado para los fines 
                                                    	que estime conveniente, sin ulterior responsabilidad para el Banco de Cr&#xE9;dito e Inversiones. 
                                                    </fo:block>
                                                </fo:table-cell>
                                            </fo:table-row>                                    
                                        </fo:table-body>
                                    </fo:table> 

                                    <fo:table table-layout="fixed" width="100%" border-collapse="collapse" padding-top="20mm">
                                        <fo:table-column column-width="proportional-column-width(15)" column-number="1"/>
                                        <fo:table-column column-width="proportional-column-width(70)" column-number="2"/>
                                        <fo:table-column column-width="proportional-column-width(15)" column-number="3"/>
                                        <fo:table-body>
                                            <fo:table-row>
                                                <fo:table-cell>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-weight="bold" font-size="14pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="center" border-top-style="solid" border-top-width="1px">
                                                    	p.p. BANCO DE CREDITO E INVERSIONES 
                                                    </fo:block>
                                                </fo:table-cell>
                                                <fo:table-cell>
                                                </fo:table-cell>
                                            </fo:table-row>                                    
                                        </fo:table-body>
                                    </fo:table> 

                                    <fo:table table-layout="fixed" width="100%" border-collapse="collapse">
                                        <fo:table-column column-width="proportional-column-width(100)" column-number="1"/>
                                        <fo:table-body>
                                            <fo:table-row>
                                                <fo:table-cell>
                                                    <fo:block font-style="oblique" font-size="12pt" font-family="Arial, Helvetica, sans-serif" space-before="2mm" text-align="left" padding-top="20mm">
														Santiago, <xsl:value-of select="dia"/> de <xsl:value-of select="mes"/> del <xsl:value-of select="ano"/>                                                	
                                                    </fo:block>
                                                </fo:table-cell>
                                            </fo:table-row>                                    
                                        </fo:table-body>
                                    </fo:table> 
                                </fo:block>
                            </fo:table-cell>
                        </fo:table-row>
                    </fo:table-body>
                </fo:table>
                
            </fo:block> 
    </xsl:template>    

</xsl:stylesheet>