	      <fo:table-body>
		 <fo:table-row>
		    <fo:table-cell >
		       <fo:block font-family="Helvetica" text-align="left" vertical-align="middle" line-height="4.5mm" font-size="11pt">1. Declaración o denuncio del siniestro.</fo:block>
		    </fo:table-cell>
		 </fo:table-row>
		 <fo:table-row>
