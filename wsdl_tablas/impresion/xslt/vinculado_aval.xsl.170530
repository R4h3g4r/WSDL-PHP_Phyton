                  <fo:table-body>
                      <fo:table-row>
                            <fo:table-cell >
                                    <fo:block   font-family="Helvetica" font-size="11pt" font-weight="bold" text-align="left" margin-left="1mm"   background-color="#BDBDBD">SEGURO DESGRAVAMEN</fo:block>
