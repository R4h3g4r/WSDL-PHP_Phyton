                  <fo:table-body>
                      <fo:table-row>

                            <fo:table-cell >
                                    <fo:block   font-family="Helvetica" font-size="11pt" font-weight="bold" text-align="left" margin-left="1mm"  background-color="#BDBDBD">SEGURO CESANTÍA (Imprimir este texto sólo si cliente contrata esta cobertura)</fo:block>
                            </fo:table-cell>


                      </fo:table-row>
                </fo:table-body>
